                end)
             
function UnClip()
    if game:GetService("Workspace"):FindFirstChild("Foot") then
        game:GetService("Workspace"):FindFirstChild("Foot"):Destroy()
    end
    if game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyClip") then
        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyClip"):Destroy()
    end
    game.Players.LocalPlayer.Character.Humanoid:ChangeState(14)
end

        task.spawn(function()
            while task.wait() do
                if _G.Ghost or _G.Trident or _G.Hell or _G.Mother or _G.Dragon or _G.Orbs or _G.Axe or _G.Knife or _G.SeaKing or _G.Cookie or _G.Sunken or _G.Pondere or _G.SoulCane or _G.KillPl or _G.Farm_OniMask or _G.TenguMask or _G.LvFarm or _G.Hydra or _G.Permission or _G.Permission2 or _G.Biscuit_Shoulder or _G.Blue_Scarf or _G.Lucidus or _G.Hydra or _G.KillRaidMon or _G.MonsFarm or _G.BlackBeard or _G.GrabGhostChest or _G.RandomDF then
                    pcall(function()
                    if not game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyClip") then
                        local Noclip = Instance.new("BodyVelocity")
                        Noclip.Name = "BodyClip"
                        Noclip.Parent = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart
                        Noclip.MaxForce = Vector3.new(100000,100000,100000)
                        Noclip.Velocity = Vector3.new(0,0,0)
                    end

                    for i, v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do
                        for k, x in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
                            v.HumanoidRootPart.CanCollide = false
                            v.Head.CanCollide = false
                            wait()
                            x.HumanoidRootPart.CanCollide = false
                            x.Head.CanCollide = false  
                        end
                    end
                 end)
end
end
end)


task.spawn(function()
    while task.wait() do
        if _G.Ghost or _G.Trident or _G.Hell or _G.Mother or _G.Dragon or _G.Orbs or _G.Axe or _G.Knife or _G.SeaKing or _G.Cookie or _G.Sunken or _G.Pondere or _G.SoulCane or _G.Farm_OniMask or _G.TenguMask or _G.LvFarm or _G.Hydra or _G.Permission or _G.Permission2 or _G.Biscuit_Shoulder or _G.Blue_Scarf or _G.Lucidus or _G.Hydra or _G.MonsFarm or _G.BlackBeard then
            pcall(function()
                if MainWeaponName == nil then
                    wait(2)
                    UI:Notify(HubName, "Please Select Your Weapon") 
                end
            end)
        end
    end
end)

