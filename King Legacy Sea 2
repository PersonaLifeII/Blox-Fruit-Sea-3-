                    task.spawn(function()
                        while task.wait() do
                            if _G.Ghost or _G.Trident or _G.Hell or _G.Mother or _G.Dragon or _G.Orbs or _G.Axe or _G.Knife or _G.SeaKing or _G.Cookie or _G.Sunken or _G.Pondere or _G.SoulCane or _G.KillPl or _G.Farm_OniMask or _G.TenguMask or _G.LvFarm or _G.Hydra or _G.Permission or _G.Permission2 or _G.Biscuit_Shoulder or _G.Blue_Scarf or _G.Lucidus or _G.Hydra or _G.KillRaidMon or _G.MonsFarm or _G.BlackBeard == true then
                                pcall(function()
                                    for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                                        if v.ClassName == "Tool" then
                                        if v.ToolTip == "Sword" then
                                          game:GetService("ReplicatedStorage").Remotes.Functions.SkillAction:InvokeServer("SW_"..MainWeaponName.."_M1") 
                                    end
                                    if v.ToolTip == "Combat" then
                                            game:GetService("ReplicatedStorage").Remotes.Functions.SkillAction:InvokeServer("FS_"..MainWeaponName.."_M1")
                                    end
                                end
                            end
                        end)
                        end
                    end
                end)

