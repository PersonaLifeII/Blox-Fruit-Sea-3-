    AutoRaidType = a
end)

RaidWeapon = {}
for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do  
    if v:IsA("Tool") then
        table.insert(RaidWeapon ,v.Name)
    end
end

RaidS:addDropdown("Select Weapon : nil", RaidWeapon,function(value)
    RaidWeaponName = value
end)

DisRaid = 13
RaidS:addSlider("Distance", 13,-50,200, function(a)
    DisRaid = a
end)
RaidS:addButton("Refresh Weapon", function(a)
    table.clear(RaidWeapon)
for i,v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
    if v:IsA("Tool") then
   table.insert(RaidWeapon,v.Name)
end
end
end)

local Raid = page5:addSection("Raid")
           ----------      ?  "if newworld then       :>"

Raid:addToggle("Auto Kill All Monster", nil, function(value)
    _G.KillRaidMon = value
    if value == false then
        UnClip()
    end
end)

    task.spawn(function()
        while task.wait() do
            if _G.KillRaidMon == true then
            pcall(function()
            if game:GetService("Workspace").MOB:GetDescendants("HumanoidRootPart") then
            for i,v in pairs(game:GetService("Workspace").MOB:GetDescendants()) do
            if v:IsA("Model") and v:FindFirstChild("Humanoid") then
                repeat game:GetService("RunService").Heartbeat:wait()
                    MinHealth = game.Players.LocalPlayer.Character.Humanoid.MaxHealth * 45 / 100
                    if game.Players.LocalPlayer.Character.Humanoid.Health > MinHealth then
                        if v.Humanoid.Health > 0 then
                        local plr = game.Players.localPlayer.Character.HumanoidRootPart
                        plr.CFrame = v.HumanoidRootPart.CFrame * Raid_Mode
                        EquipWeapon2(RaidWeaponName)
                        Haki()
                    end
                        else
                            repeat game:GetService("RunService").Heartbeat:wait()
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0,1000,0)
                            until _G.KillRaidMon == false or game.Players.LocalPlayer.Character.Humanoid.Health == game.Players.LocalPlayer.Character.Humanoid.MaxHealth
                        end
                    until not v.Parent or v.Humanoid.Health <= 0 or _G.KillRaidMon == false
                end
            end
        else
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0,1000,0)
     end
 end)
 end
 end
end)

