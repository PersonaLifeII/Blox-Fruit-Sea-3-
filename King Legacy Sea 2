        if not game:GetService("Players").LocalPlayer.Accessories:FindFirstChild("Tengu Mask") then
            if not game:GetService("Workspace").Monster.Boss:FindFirstChild("The Ice King [Lv. 3350]") or not game:GetService("ReplicatedStorage").MOB:FindFirstChild("The Ice King [Lv. 3350]") then
                local plr = game.Players.LocalPlayer.Character.HumanoidRootPart

                if not game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Ice Crystal") or not game:GetService("Players").LocalPlayer.Character:FindFirstChild("Ice Crystal") then
                    plr.CFrame = CFrame.new(6676, 69, -2756)

                    for i, v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do
                    if v.Name == "The Volcano [Lv. 3325]" and v:FindFirstChild("Humanoid") then
                    if v.Humanoid.Health > 0 then

                    local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
                    plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
                    EquipWeapon1(MainWeaponName)
                    Haki()        
                            end
                        end
                    end
                end

        if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Ice Crystal") or game:GetService("Players").LocalPlayer.Character:FindFirstChild("Ice Crystal") then
            plr.CFrame = CFrame.new(6285, 158, -3335)
            wait()
            UIClick()
        end

                        QuestLevel = 3350
                        GetUIQuest(QuestLevel)
                    end
                end
        
                if game:GetService("ReplicatedStorage").MOB:FindFirstChild("The Ice King [Lv. 3350]") then
                    plr.CFrame = CFrame.new(6285, 158, -3335)
                end

            if game:GetService("Workspace").Monster.Boss:FindFirstChild("The Ice King [Lv. 3350]") then
                for i, v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
                    if v.Name == "The Ice King [Lv. 3350]" and v:FindFirstChild("Humanoid") then
                        if v.Humanoid.Health > 0 then

                    local plr = game.Players.LocalPlayer.Character.HumanoidRootPart

                     plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
                       EquipWeapon1(MainWeaponName)
                        Haki()
                        else
                            v:Destroy() break;
                                    end
                                end
                            end
                        end
                    end)
            end
        end
    end)

