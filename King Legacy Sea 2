local FARM8 = page1:addSection("Hydra")
FARM8:addToggle("Auto Farm Hydra", nil, function(value)
    _G.Hydra = value
    if value == true then
    if game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing") then
    UI:Notify("Impact Hub", "Recommended For Distance 150 - 200")
    end
    if AutoFarmType == "[Default : Above] Auto Farm Type : Bellow" or "[Default : Above] Auto Farm Type : Above" then
        UI:Notify("Impact Hub", "This Function Is Can Be Using In Behind Type Only")
    end
elseif value == false then
    UnClip()
end
end)

task.spawn(function()
    while task.wait() do
        if _G.Hydra then
        pcall(function()
            if not game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing") then
                TPHDIsland()
            end

            local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
            for i,v in pairs(game:GetService("Workspace").SeaMonster:GetChildren()) do
                if game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing") then
                   if v.Name == "HydraSeaKing" and v:FindFirstChild("Humanoid") then
                    if v.Humanoid.Helath > 0 then
                    plr.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,80,DisFarm)
                    EquipWeapon1(MainWeaponName)
                    Haki()
                        end
                    end
                end
            end
            if _G.Farm_Lv then
                if game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing").Humanoid.Helath <= 0 then
                wait(3)
                _G.Farm_Lv = true
                else
                    _G.Farm_Lv = false
                end
            end
        end)
     end
    end
end)

