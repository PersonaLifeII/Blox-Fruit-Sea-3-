                                   table.insert(RaidWeapon,v.Name)
                                               end
                                            end
                                        end)
                                end
                            end)

                            task.spawn(function()
                                while task.wait() do
                                    pcall(function()
                                        game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed += 20
                                    end)
                                    end
                                    end)
                                    
