                while task.wait() do
                    pcall(function()
                        wait(.8)
                        table.clear(BountyWeaponList)
                        for i,v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
                            if v:IsA("Tool") then
                           table.insert(BountyWeaponList,v.Name)
                                       end
                                    end
                                end)
                        end
                    end)
                    task.spawn(function()
                        while task.wait() do
                            pcall(function()
                                wait(.8)
                                table.clear(RaidWeapon)
