local HubName = "Impact Hub V2.1"
local date = os.date("%d".." ".."%B".." ".."%Y")

local Venyx = loadstring(game:HttpGet("https://raw.githubusercontent.com/PersonaLifeII/Impact-Hub/main/UI%20Venyx"))()
local UI  = Venyx.new(HubName.." | Easily To Used | Revamp Script | "..date, 5013109572)

local page1 = UI:addPage("Main Tab", 7040391851)
local FARMS = page1:addSection("Auto Farm Setting")


spawn(function()
while wait(.1) do
    if AutoFarmType == "[Default : Above] Auto Farm Type : Above" then
        Farm_Mode = CFrame.new(0,DisFarm,-1) * CFrame.Angles(math.rad(-90),0,0)
    elseif AutoFarmType == "[Default : Above] Auto Farm Type : Bellow" then
        Farm_Mode = CFrame.new(0,DisFarm,1) * CFrame.Angles(math.rad(90),0,0)
    elseif AutoFarmType == "[Default : Above] Auto Farm Type : Behind" then
        Farm_Mode = CFrame.new(0,0,DisFarm) * CFrame.Angles(math.rad(0),0,0)
    end
end
end)

FarmTable = {
"[Default : Above] Auto Farm Type : Above",
"[Default : Above] Auto Farm Type : Bellow",
"[Default : Above] Auto Farm Type : Behind"
}

AutoFarmType = FarmTable[1]

FARMS:addDropdown(""..AutoFarmType, FarmTable, function(value)
    AutoFarmType = value
    if value == "[Default : Above] Auto Farm Type : Above" then

        UI:Notify(HubName, "To Use This Type.\nYou Must Sliding The Distance More Than 0")
    elseif value == "[Default : Above] Auto Farm Type : Bellow" then

        UI:Notify(HubName, "To Use This Type.\nYou Must Sliding The Distance Lower Than 0")
    end
end)

MainWeaponList = {}
for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do  
    if v:IsA("Tool") then
        table.insert(MainWeaponList ,v.Name)
    end
end

FARMS:addDropdown("Select Weapon : None", MainWeaponList, function(value)
    MainWeaponName = value
end)

_G.GetQuest = true
FARMS:addToggle("Auto Farm Receive Quest", true, function(value)
    _G.GetQuest = value
end)

FARMS:addToggle("White Screen", nil, function(value)
    WhiteScreen = value
    if WhiteScreen then
    game:GetService("RunService"):Set3dRenderingEnabled(false)

    else
        game:GetService("RunService"):Set3dRenderingEnabled(true)
        UnClip()
    end
    
end)


DisFarm = 13
FARMS:addSlider("Distance", 13,-50,200, function(a)
    DisFarm = a
end)

local FARM = page1:addSection("Auto Farm")

FARM:addToggle("Auto Farm Level", nil, function(value)
    _G.LvFarm = value
if value == true then
    MonSelected = nil
else
UnClip()
CheckQuest()
end
end)

task.spawn(function()
    while task.wait() do
    if _G.LvFarm then
    pcall(function()
local plr = game.Players.LocalPlayer.Character.HumanoidRootPart

    EquipWeapon1(MainWeaponName)
    Haki()

    CheckQuest()
    if not game:GetService("Workspace").Monster.Mon:FindFirstChild(Ms) then
    if not game:GetService("Workspace").Monster.Boss:FindFirstChild(Ms) then
    CheckQuest()
    plr.CFrame = CFrameMon
        end
    end

if _G.GetQuest then
    CheckQuest()
    if game:GetService("Players").LocalPlayer.PlayerGui.Quest.QuestBoard.Visible == false then
        QuestLevel = levelquest

        if game:GetService("Workspace").AllNPC:FindFirstChild("QuestLvl"..QuestLevel) then
            plr.CFrame = game:GetService("Workspace").AllNPC["QuestLvl"..QuestLevel].HumanoidRootPart.CFrame * CFrame.new(0,-5,0)
            wait()

            UIClick()

        else 
            CheckQuest()
            QuestLevel = levelquest
            plr.CFrame = game:GetService("ReplicatedStorage").MAP["QuestLvl"..QuestLevel].HumanoidRootPart.CFrame * CFrame.new(0,-5,0)
            wait()

            UIClick()
         end    

         CheckQuest()
         QuestLevel = levelquest
         GetUIQuest(QuestLevel)
end

CheckQuest()
if game:GetService("Players").LocalPlayer.PlayerGui.Quest.QuestBoard.Visible == true then

    CheckQuest()
    if not game:GetService("Workspace").Monster.Mon:FindFirstChild(Ms) then
    if not game:GetService("Workspace").Monster.Boss:FindFirstChild(Ms) then
    CheckQuest()
    plr.CFrame = CFrameMon
        end
    end

    CheckQuest()
if game:GetService("Workspace").Monster.Mon:FindFirstChild(Ms) then
    for i, v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do
      if v.Name == Ms then
        if v.Humanoid.Health > 0 then
        plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
    elseif v.Humanoid.Health <= 0 then
        v:Destroy() break;
        end
    end
end
    CheckQuest()
    elseif game:GetService("Workspace").Monster.Boss:FindFirstChild(Ms) then
            for i, v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
              if v.Name == Ms then
                if v.Humanoid.Health > 0 then
                plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode

                elseif v.Humanoid.Health <= 0 then
                    v:Destroy() break;
                            end                    
                        end
                    end
                end  
            end
        end

if not _G.GetQuest then
CheckQuest()
if game:GetService("Workspace").Monster.Mon:FindFirstChild(Ms) then
    for i, v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do
      if v.Name == Ms then
        if v.Humanoid.Health > 0 then
        plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
    elseif v.Humanoid.Health <= 0 then
        v:Destroy() break;
        end
    end
end
    CheckQuest()
    elseif game:GetService("Workspace").Monster.Boss:FindFirstChild(Ms) then
            for i, v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
              if v.Name == Ms then
                if v.Humanoid.Health > 0 then
                plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode

                elseif v.Humanoid.Health <= 0 then
                    v:Destroy() break;
                            end                    
                        end
                    end
                end  
            end
        end)
end
end
end)

function GetUIQuest(QuestLevel)
    if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("QuestLvl"..QuestLevel) then
      if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("QuestLvl"..QuestLevel).Dialogue then
          if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("QuestLvl"..QuestLevel).Dialogue.Accept then

              game:GetService("Players").LocalPlayer.PlayerGui["QuestLvl"..QuestLevel].Dialogue.Accept.Size = UDim2.new(0, 10000, 0, 10000)
              game:GetService("Players").LocalPlayer.PlayerGui["QuestLvl"..QuestLevel].Dialogue.Accept.Position = UDim2.new(-2, 0, -5, 0)
              game:GetService("Players").LocalPlayer.PlayerGui["QuestLvl"..QuestLevel].Dialogue.Accept.ImageTransparency = 1
          end
      end
  end
end


local mobs = {} 

for _,v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do
    insert = true 
    for _,v2 in pairs(mobs) do
    if v2 == v.Name then
    insert = false 
    end
    end 
if insert then
 table.insert(mobs, v.Name)
 end 
end


local mobdropdown = FARM:addDropdown("Select Monster : None", mobs, function(value)
    MonSelected = value
end)


FARM:addToggle("Auto Farm Monster", nil, function(value)
    _G.MonsFarm = value
    if value == false then
        UnClip()
    end
end)
 
task.spawn(function()
    while task.wait() do
        if _G.MonsFarm then
        pcall(function()
        local plr = game.Players.LocalPlayer.Character.HumanoidRootPart

        if MonSelected == nil then
            wait(2)
            UI:Notify(HubName, "Please Select The Monster")
        end
        
        CheckQuest()
        if not game:GetService("Workspace").Monster.Mon:FindFirstChild(Ms) then
            plr.CFrame = CFrameMon
        end

if _G.GetQuest then

        CheckQuest()
        if game:GetService("Players").LocalPlayer.PlayerGui.Quest.QuestBoard.Visible == false then
            QuestLevel = levelquest
    
            if game:GetService("Workspace").AllNPC:FindFirstChild("QuestLvl"..QuestLevel) then
                plr.CFrame = game:GetService("Workspace").AllNPC["QuestLvl"..QuestLevel].HumanoidRootPart.CFrame * CFrame.new(0,-5,0)
                wait()
    
                UIClick()
    
            else 
                QuestLevel = levelquest
                plr.CFrame = game:GetService("ReplicatedStorage").MAP["QuestLvl"..QuestLevel].HumanoidRootPart.CFrame * CFrame.new(0,-5,0)
                wait()
    
                UIClick()
             end    
    
             CheckQuest()
             QuestLevel = levelquest
             GetUIQuest(QuestLevel)
    end

    if game:GetService("Players").LocalPlayer.PlayerGui.Quest.QuestBoard.Visible == true then

        if game:GetService("Workspace").Monster.Mon:FindFirstChild(Ms) then
            for i, v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do
            if v.Name == Ms and v:FindFirstChild("Humanoid") then
            if v.Humanoid.Health > 0 then
            plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
            EquipWeapon1(MainWeaponName)
            Haki()
            else
                v:Destroy() break;
                            end
                        end
                    end
                end
            end
        end

if not _G.GetQuest then
    CheckQuest()
        if game:GetService("Workspace").Monster.Mon:FindFirstChild(Ms) then
            for i, v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do
            if v.Name == Ms and v:FindFirstChild("Humanoid") then
            if v.Humanoid.Health > 0 then
            plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
            EquipWeapon1(MainWeaponName)
            Haki()
            else
                v:Destroy() break;
                    end
                end
            end
        end
    end
end)
end
end
end)
            
            game:GetService("Workspace").Monster.Mon.ChildAdded:Connect(function() 
                for _,v2 in pairs(mobs) do
                 table.remove(mobs, _)
             end
                
                for _,v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do
                    insert = true 
                    for _,v2 in pairs(mobs) do
                     if v2 == v.Name then
                     insert = false end
                     end
                    if insert then
                 table.insert(mobs, v.Name) end 
                end
                mobdropdown:Refresh(mobs)
            end)
        
   

    FARM:addButton("Check Server Time", function()
        local GameTime = math.floor(workspace.DistributedGameTime+0.5)
        local Hour = math.floor(GameTime/(60^2))%24
        local Minute = math.floor(GameTime/(60^1))%60
        local Second = math.floor(GameTime/(60^0))%60

         UI:Notify("Impact Hub", "Total Time You Have Joined\nHour : "..Hour.." Minute : "..Minute.." Second : "..Second);
    end)

    FARM:addButton("Teleport To Current Quest", function()
        CheckQuest()
        if not game:GetService("Workspace").AllNPC:FindFirstChild("QuestLvl"..levelquest) then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("ReplicatedStorage").MAP["QuestLvl"..levelquest].HumanoidRootPart.CFrame
         else 
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").AllNPC["QuestLvl"..levelquest].HumanoidRootPart.CFrame
         end
    end)

    _G.Buso = true
    FARM:addToggle("Auto Armament Haki",  true, function(a)
        _G.Buso = a
    end)

task.spawn(function()
    while task.wait() do
        if _G.Buso then
        pcall(function()
        Haki()
        end)
    end
end
end)

    FARM:addToggle("Auto Observation Haki",  nil, function(a)
        _G.KenHaki = a
    end)

task.spawn(function()
    while task.wait() do
        if _G.KenHaki then
        pcall(function()
        KenHaki()
        end)
    end
end
end)

