                    game:GetService("ReplicatedStorage").Remotes.Functions.SkillAction:InvokeServer(unpack(args))
                    elseif RaidWeaponName == "Combat" or RaidWeaponName == "DarkLeg" or RaidWeaponName == "WaterStyle" or RaidWeaponName == "Cyborg" or RaidWeaponName == "DragonClaw" or RaidWeaponName == "Electro" or RaidWeaponName == "" then
                        local args = {
