    pcall(function()
        local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
        if game:GetService("ReplicatedStorage").MOB:FindFirstChild("Dark Beard [Lv. 3475]")  then
        plr.CFrame = CFrame.new(-10160, 145, -5140)
        for i, v in pairs(game.Workspace.Monster.Boss:GetChildren()) do
            if v.Name == "Dark Beard [Lv. 3475]" and v:FindFirstChild("Humanoid") then
                plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
                EquipWeapon1(MainWeaponName)
                Haki()
                if not game:GetService("ReplicatedStorage").MOB:FindFirstChild("Dark Beard [Lv. 3475]") or v.Humanoid.Health <= 0 then
                    plr.CFrame = CFrame.new(-10160, 145, -5140)
                  end
                end
            end
        end
    end)
    end
end
end)
