local RaidS = page5:addSection("Raid Setting")
    
AutoRaidType = "[Default : Above] Type Farm : Above"
spawn(function()
while wait(.1) do
    if AutoRaidType == "[Default : Above] Type Farm : Above" then
        Raid_Mode = CFrame.new(0,DisRaid,-2) * CFrame.Angles(math.rad(-90),0,0)
    elseif AutoRaidType == "[Default : Above] Type Farm : Bellow" then
        Raid_Mode = CFrame.new(0,DisRaid,2) * CFrame.Angles(math.rad(90),0,0)
    elseif AutoRaidType == "[Default : Above] Type Farm : Behind" then
        Raid_Mode = CFrame.new(0,0,DisRaid) * CFrame.Angles(math.rad(0),0,0)
    end
end
end)

RaidTable = {"[Default : Above] Auto Raid Type : Above","[Default : Above] Auto Raid Type : Above", "[Default : Above] Auto Raid Type : Behind", "[Default : Above] Auto Raid Type : Front"}
