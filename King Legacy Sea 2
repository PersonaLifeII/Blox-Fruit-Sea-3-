  TPGAYIsland()
end)

local FARM2 = page1:addSection("Hydra")

FARM2:addToggle("Auto Farm Hydra", nil, function(value)
    _G.Hydra = value
    if value == true then
    if game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing") then
    UI:Notify(HubName, "Recommended For Distance 150 - 200")
    end 
    if AutoFarmType == "[Default : Above] Auto Farm Type : Bellow" or "[Default : Above] Auto Farm Type : Above" then
        UI:Notify(HubName, "This Function Is Can Be Using In Behind Type Only")
    end
elseif value == false then
    UnClip()
end
end)

task.spawn(function()
    while task.wait() do
        if _G.Hydra then
        pcall(function()
            if not game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing") then
            TPHDIsland()
            end

            local plr = game.Players.LocalPlayer.Character.HumanoidRootPart

            if game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing") then
            for i,v in pairs(game:GetService("Workspace").SeaMonster:GetChildren()) do

                if game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing") then
                   if v.Name == "HydraSeaKing" and v:FindFirstChild("Humanoid") then
                    if v.Humanoid.Helath > 0 then
                    plr.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,80,DisFarm)
                    EquipWeapon1(MainWeaponName)
                    Haki()
                            end
                        end
                    end
                end
            end
        end)
     end
    end
end)

FARM2:addToggle("Auto Farm Hydra Hop", nil, function(value)
    _G.HdHop = value

    if value == true then
    if _G.Hydra == false then
    UI:Notify(HubName, "Turn On The Auto Farm Hydra Function")
    end
end
end)

task.spawn(function()
    while task.wait() do
        if _G.HdHop then
        pcall(function()
            local Ws = game:GetService("Workspace").Island
            local plr = game.Players.LocalPlayer.Character.HumanoidRootPart

            for i, v in pairs(Ws:GetChildren()) do
            if string.find(v.Name, "Sea King") then
            if game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing") then
            if (game:GetService("Workspace").SeaMonster:FindFirstChild("HydraSeaKing").HumanoidRootPart.Position - plr.Position).Magnitude > 10000 then 
            print("The Hydra Are Got Bug")
            UI:Notify(HubName, "[Hydra Buged] Hoping....")
            wait(2)
            Hop()
            end
        end
    end
end
end)
end
end
end)

task.spawn(function()
    while task.wait() do
        if _G.HdHop then
        pcall(function()
            local Ws = game:GetService("Workspace").Island

            for i, v in pairs(Ws:GetChildren()) do
            if string.find(v.Name, "Sea King") then
            print("gay")
            else
                print("Not Hydra Island")
                UI:Notify(HubName, "[Not Found Hydra Island] Hoping.......")
                wait(3)
                Hop()
                    end
                end
            end)
        end
    end
end)


FARM2:addToggle("Auto Grab Chest", nil, function(value)
    _G.GrabHydraChest2 = value
end)

task.spawn(function()
    while task.wait() do
        if _G.GrabHydraChest2 then
        pcall(function()

            local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
            local Ws = game:GetService("Workspace").Island
            if Ws:FindFirstChild("Sea King Water") or Ws:FindFirstChild("Sea King Thunder") or Ws:FindFirstChild("Sea King Lava") then

            for i, v in pairs(game.Workspace.Island:GetChildren()) do
            if v.Name == "Sea King Water" or "Sea King Thunder" or "Sea King Lava" and v:FindFirstChild("Main") then
            if string.find(v.Name, "Chest") then
            plr.CFrame = v.RootPart.CFrame * CFrame.new(0,2,0)
            end
        end
    end
end
end)
end
    end
end)


