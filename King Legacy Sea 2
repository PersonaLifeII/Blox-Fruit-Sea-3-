_G.BossNotify = true
FARM_NOTIFY:addToggle("Boss Notify", true, function(value)
    _G.BossNotify = value
    if value == false then
    _G.SoundCheck = false
    end
end)

task.spawn(function()
    while task.wait() do
        if _G.BossNotify then
        pcall(function()
                 FindGayIsland()
               FindHDIsland()
              FindGS()
            FindRaidBoss()
        end)
     end
    end
end)

FARM_NOTIFY:addToggle("Boss Notify Sound", nil, function(value)
    _G.BossNotify_SoundAdd = value
    if value == false then
        _G.SoundCheck = false

for i, v in pairs(game:GetService("Workspace"):GetChildren()) do
    if v.Name == "Boss_Notify" and v:IsA("Sound") then
        v:Destroy()
        end
    end
end
    end)

task.spawn(function()
while task.wait() do
if _G.BossNotify then
pcall(function()
if _G.BossNotify_SoundAdd then

if _G.SoundCheck == true then
wait(3)
so = Instance.new("Sound", game:GetService("Workspace"))
so.Name = "Boss_Notify"
so.Playing = _G.BossNotify_SoundAdd
so.Volume = 4
so.SoundId = "rbxassetid://1523818134"
so.Looped = false
end
end
end)
end
end
end)


local FARM1 = page1:addSection("Sea King")

FARM1:addToggle("Auto Farm Sea King", nil, function(value)
    _G.SeaKing = value
    if value == true then
    if game:GetService("Workspace").SeaMonster:FindFirstChild("SeaKing") then

        local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
    if (game:GetService("Workspace").SeaMonster:FindFirstChild("SeaKing").HumanoidRootPart.Position - plr.Position).Magnitude < 15000 then
        UI:Notify(HubName, "Recomeded For Distance 200")
    else
        UI:Notify(HubName, "Sea King Is Buging. Turn Off The Function")
        end
    end

    elseif value == false then
        UnClip()
      end
end)

task.spawn(function()
    while task.wait() do
        if _G.SeaKing then
        pcall(function()
            local plr = game.Players.LocalPlayer.Character.HumanoidRootPart

        if not game:GetService("Workspace").SeaMonster:FindFirstChild("SeaKing") then
            TPGAYIsland()
        end

        if game:GetService("Workspace").SeaMonster:FindFirstChild("SeaKing") then
            for i,v in pairs(game:GetService("Workspace").SeaMonster:GetChildren()) do
                   if v.Name == "SeaKing" and v:FindFirstChild("Humanoid") then
                    if v.Humanoid.Health > 0 then
                    if (v.HumanoidRootPart.Position - plr.Position).Magnitude < 15000 then

                    plr.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,DisFarm)
                    EquipWeapon1(MainWeaponName)
                    Haki()
                        end
                    end
                end
            end
        end
    end)
     end
    end
end)

FARM1:addToggle("Auto Farm Sea King Hop", nil, function(value)
    _G.SkHop = value
    if value == true then
    end
end)

task.spawn(function()
    while task.wait() do
        if _G.SkHop then
        pcall(function()
            local plr = game.Players.LocalPlayer.Character.HumanoidRootPart

            if game:GetService("Workspace").SeaMonster:FindFirstChild("SeaKing") then
            if (game:GetService("Workspace").SeaMonster:FindFirstChild("SeaKing").HumanoidRootPart.Position - plr.Position).Magnitude > 15000 then 
            print("The Sea King Are Got Bug")
            UI:Notify(HubName, "[Sea King Buged] Hoping....")
            wait(2)
            Hop()
    end
end
end)
end
end
end)

    task.spawn(function()
    while task.wait() do
    if _G.SkHop then
    pcall(function()
local Ws = game:GetService("Workspace").Island

            if not Ws:FindFirstChild("Legacy Island1") then
            if not Ws:FindFirstChild("Legacy Island2") then
            if not Ws:FindFirstChild("Legacy Island3") then
            if not Ws:FindFirstChild("Legacy Island4") then

            print("Not Legacy Island")
            UI:Notify(HubName, "[Not found Legacy Island] Hoping.......")
            wait(3)
            Hop()

                        end
                    end
                end
            end
        end)
    end
end
end)

FARM1:addToggle("Auto Grab Chest", nil, function(a)
    _G.GrabChest = a
end)

    task.spawn(function()
        while task.wait() do
            if _G.GrabChest then
            pcall(function()
                local plr = game.Players.LocalPlayer.Character.HumanoidRootPart

                if game:GetService("Workspace").SeaMonster:FindFirstChild("SeaKing") then
                    if game:GetService("Workspace").SeaMonster:FindFirstChild("SeaKing").Humanoid.Health <= 0 then

                    if game:GetService("Workspace").Island:FindFirstChild("Legacy Island1") then
                        plr.CFrame = game:GetService("Workspace").Island["Legacy Island1"].ChestSpawner.CFrame

                    elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island2") then
                        plr.CFrame = game:GetService("Workspace").Island["Legacy Island2"].ChestSpawner.CFrame

                    elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island3") then
                        plr.CFrame = game:GetService("Workspace").Island["Legacy Island3"].ChestSpawner.CFrame

                    elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island4") then
                        plr.CFrame = game:GetService("Workspace").Island["Legacy Island4"].ChestSpawner.CFrame
                      end
                    end
                end
            end)
     end
    end
end)


