    _G.SafeRaid = value
    if value == false then
        UnClip()
    end
end)

task.spawn(function()
    while task.wait() do
        if _G.KillRaidMon then
        pcall(function()
            local plr = game.Players.localPlayer.Character

            for i, v in pairs(game:GetService("Workspace").MOB:GetChildren()) do
                if v:IsA("Model") and v:FindFirstChild("Humanoid") then
                if v.Humanoid.Health > 0 then

                if _G.SafeRaid then
                MinHealth = game.Players.LocalPlayer.Character.Humanoid.MaxHealth * 50 / 100
                if plr.Humanoid.Health > MinHealth then
                    plr.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * Raid_Mode
                    EquipWeapon2(RaidWeaponName)
                    Haki()
                                end
                            end
                            plr.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * Raid_Mode
                            EquipWeapon2(RaidWeaponName)
                            Haki()
                        end
                    end
                end
            end)
    end
end
end)

