local Character = page6:addSection("Character")

Character:addToggle("Auto Click", nil, function(value)
_G.AutoC = value
end)
 
spawn(function()
    game:GetService("RunService").RenderStepped:Connect(function()
     pcall(function()
         if _G.AutoC then
game:GetService'VirtualUser':CaptureController()
game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672))
        end
 end)
end)
end)

FlySpeed = 250
Character:addSlider("Fly Speed", 250,1,1250, function(value)
FlySpeed = value
end)

FlyBind = "g"
Character:addTextbox("[Only Small Text] Fly Key", "g", function(value)
    FlyBind = value
end)

        Character:addToggle("Fly", nil, function(value)
        FL = value
        local Max = 0
        local Players = game.Players
        local LP = Players.LocalPlayer
        local Mouse = LP:GetMouse()
        Mouse.KeyDown:connect(function(k)
                if k:lower() == FlyBind then
                    Max = Max + 1
                    getgenv().Fly = false
                    if FL then
                        local T = LP.Character.UpperTorso
                        local S = {
                            F = 0,
                            B = 0,
                            L = 0,
                            R = 0
                        }
                        local S2 = {
                            F = 0,
                            B = 0,
                            L = 0,
                            R = 0
                        }
                        local SPEED = 65
                        local function FLY()
                            getgenv().Fly = true
                            local BodyGyro = Instance.new("BodyGyro", T)
                            local BodyVelocity = Instance.new("BodyVelocity", T)
                            BodyGyro.P = 9e4
                            BodyGyro.maxTorque = Vector3.new(9e9, 9e9, 9e9)
                            BodyGyro.cframe = T.CFrame
                            BodyVelocity.velocity = Vector3.new(0, 0.1, 0)
                            BodyVelocity.maxForce = Vector3.new(9e9, 9e9, 9e9)
                            spawn(function()
                                    repeat wait()
                                        LP.Character.Humanoid.PlatformStand = false
                                        if S.L + S.R ~= 0 or S.F + S.B ~= 0 then
                                            SPEED = FlySpeed -- 200
                                        elseif not (S.L + S.R ~= 0 or S.F + S.B ~= 0) and SPEED ~= 0 then
                                            SPEED = 0
                                        end
                                        if (S.L + S.R) ~= 0 or (S.F + S.B) ~= 0 then
                                            BodyVelocity.velocity =
                                                ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (S.F + S.B)) +
                                                ((game.Workspace.CurrentCamera.CoordinateFrame *
                                                    CFrame.new(S.L + S.R, (S.F + S.B) * 0.2, 0).p) -
                                                    game.Workspace.CurrentCamera.CoordinateFrame.p)) *
                                                SPEED
                                            S2 = {
                                                F = S.F,
                                                B = S.B,
                                                L = S.L,
                                                R = S.R
                                            }
                                        elseif (S.L + S.R) == 0 and (S.F + S.B) == 0 and SPEED ~= 0 then
                                            BodyVelocity.velocity =
                                                ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (S2.F + S2.B)) +
                                                ((game.Workspace.CurrentCamera.CoordinateFrame *
                                                    CFrame.new(S2.L + S2.R, (S2.F + S2.B) * 0.2, 0).p) -
                                                    game.Workspace.CurrentCamera.CoordinateFrame.p)) *
                                                SPEED
                                        else
                                            BodyVelocity.velocity = Vector3.new(0, 0.1, 0)
                                        end
                                        BodyGyro.cframe = game.Workspace.CurrentCamera.CoordinateFrame
                                    until not getgenv().Fly
                                    S = {
                                        F = 0,
                                        B = 0,
                                        L = 0,
                                        R = 0
                                    }
                                    S2 = {
                                        F = 0,
                                        B = 0,
                                        L = 0,
                                        R = 0
                                    }
                                    SPEED = 0
                                    BodyGyro:destroy()
                                    BodyVelocity:destroy()
                                    LP.Character.Humanoid.PlatformStand = false
                                end
                            )
                        end
                        Mouse.KeyDown:connect(
                            function(k)
                                if k:lower() == "w" then
                                    S.F = 1
                                elseif k:lower() == "s" then
                                    S.B = -1
                                elseif k:lower() == "a" then
                                    S.L = -1
                                elseif k:lower() == "d" then
                                    S.R = 1
                                end
                            end
                        )
                        Mouse.KeyUp:connect(function(k)
                                if k:lower() == "w" then
                                    S.F = 0
                                elseif k:lower() == "s" then
                                    S.B = 0
                                elseif k:lower() == "a" then
                                    S.L = 0
                                elseif k:lower() == "d" then
                                    S.R = 0
                                end
                            end
                        )
                        FLY()
                        if Max == 2 then
                            getgenv().Fly = false
                            Max = 0
                        end
                    end
                end
            end
        )
 
        end)
        Character:addToggle("Noclip", nil, function(value)
            _G.Noclip = value
            end)
        Character:addToggle("Float", nil, function(value)
            _G.Float = value
            if value == false then
                UnClip()
              end
            end)
            Character:addToggle("Auto Accept Allies", nil, function(value)
                _G.Accept = value
             end)

             spawn(function()
                game:GetService("RunService").RenderStepped:Connect(function()
                 pcall(function()
                     if _G.Accept then
                        local args = {[1] = true}
                        game:GetService("Players").LocalPlayer.PlayerGui.Ally.Frame.RemoteEvent:FireServer(unpack(args))                           
                    end
             end)
            end)
            end)

            Character:addToggle("Auto Delice Allies", nil, function(value)
                _G.Delice = value
             end)

             spawn(function()
                game:GetService("RunService").RenderStepped:Connect(function()
                 pcall(function()
                     if _G.Delice then
                        local args = {[1] = false}
                        game:GetService("Players").LocalPlayer.PlayerGui.Ally.Frame.RemoteEvent:FireServer(unpack(args))                           
                    end
             end)
            end)
            end)

            Character:addButton("No Dodge Cooldown", function()
                local a;
                a = hookfunc(getrenv().wait, function(x)
                   if tostring(getfenv(2).script) == "Dash" then
                      return game:GetService("RunService").RenderStepped:wait()
                   end
                   return a(x)
                    end)
             end)
        Character:addButton("Full Bright", function()
            loadstring(game:HttpGet("https://pastebin.com/raw/06iG6YkU", true))()
        end)
        Character:addButton("Beautiful Graphic", function()
               YourHeartIsSoBeautiful()
        end)

        Character:addToggle("Anti AFK", true ,function(vu)
            local vu = game:GetService("VirtualUser")
            game:GetService("Players").LocalPlayer.Idled:connect(function()
                vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
                wait(1)
                vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
            end)
        end)

        HakiColorlist = {
            "White",
            "Lime",
            "Green",
            "",
            "Yellow",
            "Carmine",
            "Fuchia",
            "",
            "Ultramarine",
            "Azure",
            "Apricot",
            "",
            "Indigo",
            "Arc",  
            ""
        }

        HakiColor = "White"
        Character:addDropdown("Select Armament Shade : "..HakiColor, HakiColorlist, function(a)
            HakiColor = a
        end)  

        Character:addButton("Change Armament Shade", function(a)
            game:GetService("Players").LocalPlayer.PlayerStats.ArmamentColor.Value = HakiColor
            print(HakiColorlist)
        end)

        HopAndRejoinSec = 10
        Character:addSlider("Hop And Rejoin Delay", 10,5,200, function(a)
            HopAndRejoinSec = a
        end)
        Character:addToggle("Auto Hop",nil,function(vu)
           _G.AutoHop = vu
        end)
        Character:addToggle("Auto Rejoin",nil,function(vu)
            _G.AutoRejoin = vu
        end)

        task.spawn(function()
            while task.wait() do
                if _G.AutoHop then
                pcall(function()
                    wait(HopAndRejoinSec)
                    Hop()
                end)
             end
            end
        end)

        task.spawn(function()
            while task.wait() do
                if _G.AutoRejoin then
                pcall(function()
                    wait(HopAndRejoinSec)
                    Rejoin()
                end)
             end
            end
        end)


 local page7 = UI:addPage("Bounty\nShop", 7251993295)
