 FarmBountyType = "[Default : Above] Auto Farm Type : Above"
 spawn(function()
 while wait(.1) do
     if FarmBountyType == "[Default : Above] Auto Farm Type : Above" then
        Bounty_Mode = CFrame.new(0,DisBounty,-2) * CFrame.Angles(math.rad(-90),0,0)
     elseif FarmBountyType == "[Default : Above] Auto Farm Type : Bellow" then
        Bounty_Mode = CFrame.new(0,DisBounty,2) * CFrame.Angles(math.rad(90),0,0)
     elseif FarmBountyType == "[Default : Above] Auto Farm Type : Behind" then
        Bounty_Mode = CFrame.new(0,0,DisBounty) * CFrame.Angles(math.rad(0),0,0)
     end
 end
 end)
 
