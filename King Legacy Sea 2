            while task.wait() do
         if _G.Orbs then
            pcall(function()
            local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
        
            EquipWeapon1(MainWeaponName)
            Haki()
        
        if not game:GetService("ReplicatedStorage").MOB:FindFirstChild("Dragon [Lv. 5000]") and 
        not game:GetService("Workspace").Monster.Boss:FindFirstChild("Dragon [Lv. 5000]") then

        if not game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("DragonGem") and 
        not game:GetService("Players").LocalPlayer.Character:FindFirstChild("DragonGem") then

            if not game:GetService("Workspace").Monster.Boss:FindFirstChild("Elite Skeleton [Lv. 3100]") then
                plr.CFrame = CFrame.new(-6122, 165, 7168)
            end

        if _G.GetQuest then
            if game:GetService("Players").LocalPlayer.PlayerGui.Quest.QuestBoard.Visible == false then

                QuestLevel = 3075
                if game:GetService("Workspace").AllNPC:FindFirstChild("QuestLvl"..QuestLevel) then
                    plr.CFrame = game:GetService("Workspace").AllNPC["QuestLvl"..QuestLevel].HumanoidRootPart.CFrame * CFrame.new(0,-5,0)
                    wait()

                    UIClick()
                else 

                    QuestLevel = 3075
                    plr.CFrame = game:GetService("ReplicatedStorage").MAP["QuestLvl"..QuestLevel].HumanoidRootPart.CFrame * CFrame.new(0,-5,0)
                    wait()

                    UIClick()
                 end    
        
        
                 CheckQuest()
                 QuestLevel = 3075
                 GetUIQuest(QuestLevel)
        end

        if game:GetService("Players").LocalPlayer.PlayerGui.Quest.QuestBoard.Visible == true then
            if game:GetService("Workspace").Monster.Boss:FindFirstChild("Elite Skeleton [Lv. 3100]") then
                    for i, v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do

                      if v.Name == "Elite Skeleton [Lv. 3100]" then
                        if v.Humanoid.Health > 0 then
                        plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode

                        else
                            v:Destroy() break;
                                    end
                                end
                            end
                        end
                    end
                end  
            end
        end

        if not _G.GetQuest then
            if game:GetService("Workspace").Monster.Boss:FindFirstChild("Elite Skeleton [Lv. 3100]") then
                    for i, v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do

                      if v.Name == "Elite Skeleton [Lv. 3100]" then
                        if v.Humanoid.Health > 0 then
                        plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode

                        else
                            v:Destroy() break;
                                end
                            end
                        end
                    end
                end
            end)  
        end
        end
        end)



        FARM6:addToggle("Auto Summon Dragon", nil, function(value)
            _G.SummonDragon = value
            if value == false then
                UnClip()
            end
        end)
        
            task.spawn(function()
