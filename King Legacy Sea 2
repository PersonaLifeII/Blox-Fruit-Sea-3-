 local PLYS = PLY:addSection("Players")

local playerlist = {}

for i,v in pairs(game.Players:GetPlayers())do
    if v ~= game.Players.LocalPlayer then
        table.insert(playerlist,v.Name)
    end
end

local drop = PLYS:addDropdown("Select Players : nil", playerlist, function(a)
   Select = a
end)

game.Players.PlayerAdded:Connect(function(player)
    local name = player.Name
    table.insert(playerlist,name)
    drop:Refresh(playerlist)
end)

game.Players.PlayerRemoving:Connect(function(player)
    local name = player.Name
    for i,v in pairs(playerlist)do
        if v == name then  
            table.remove(playerlist,i)
        end
    end
    drop:Refresh(playerlist)
end)
PLYS:addToggle("Spectate Player", nil, function(State)
    SeeYouStatus = State
    local plr1 = game.Players.LocalPlayer.Character.Humanoid
    local plr2 = game.Players:FindFirstChild(Select)
    repeat wait(.1)
       game.Workspace.Camera.CameraSubject = plr2.Character.Humanoid
    until SeeYouStatus == false 
    game.Workspace.Camera.CameraSubject = game.Players.LocalPlayer.Character.Humanoid
 end)
 PLYS:addToggle("Kill Player", nil, function(Value)
 _G.KillPl = Value
 if Value == false then
    UnClip()
 end
 if Select == nil then
    UI:Notify("Impact Hub", "Please Select Players")
 end
 if PlyWeaponName == nil then
    UI:Notify("Impact Hub", "Please Select Weapon")
 end
         end)

         task.spawn(function()
            while task.wait() do
                if _G.KillPl then
                pcall(function()
                    if game.Players:FindFirstChild(Select) then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[Select].Character.HumanoidRootPart.CFrame * CFrame.new(0,0,DisPlayer)
                    EquipWeapon3(PlyWeaponName)
                    Haki()
                  end
                end)
             end
            end
        end)
PLYS:addToggle("Safe Mode", nil, function(a)
    _G.SafeModeOn = a
end)

