        pcall(function()
            while wait() do
                if _G.Ghost or _G.Trident or _G.Hell or _G.Mother or _G.Dragon or _G.Orbs or _G.Axe or _G.Knife or _G.SeaKing or _G.Float or _G.Cookie or _G.Sunken or _G.KillPl or _G.OniMask or _G.TenguMask or _G.LvFarm or _G.Hydra or _G.Permission or _G.Permission2 or _G.Biscuit_Shoulder or _G.Blue_Scarf or _G.Lucidus or _G.Hydra or _G.PermissionQ or _G.Permission2Q or _G.OrbsQ or _G.KillRaidMon == true then
                    if not game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyClip") then
                        local Noclip = Instance.new("BodyVelocity")
                        Noclip.Name = "BodyClip"
                        Noclip.Parent = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart
                        Noclip.MaxForce = Vector3.new(100000,100000,100000)
                        Noclip.Velocity = Vector3.new(0,0,0)
                    end
                  end
            end
        end)
    end)
    
    spawn(function()
        pcall(function()
            game:GetService("RunService").Stepped:Connect(function()
                if _G.Noclip or _G.Ghost or _G.Trident or _G.Hell or _G.Mother or _G.Dragon or _G.Orbs or _G.Axe or _G.Knife or _G.SeaKing or _G.Float or _G.Cookie or _G.Sunken or _G.KillPl or _G.OniMask or _G.TenguMask or _G.LvFarm or _G.Hydra or _G.Permission or _G.Permission2 or _G.Biscuit_Shoulder or _G.Blue_Scarf or _G.Lucidus or _G.Hydra or _G.PermissionQ or _G.Permission2Q or _G.OrbsQ or _G.GrabHydraChest2 == true then
                    for _, v in pairs(game:GetService("Players").LocalPlayer.Character:GetDescendants()) do
                        if v:IsA("BasePart") then
                            v.CanCollide = false    
                        end
                    end
                end
            end)
        end)
    end)

    spawn(function()
        pcall(function()
            game:GetService("RunService").Stepped:Connect(function()
                if _G.Buso == true then
                                Haki()   
                        end
                    end)
            end)
        end)

    spawn(function()
        pcall(function()
            game:GetService("RunService").Stepped:Connect(function()
                if _G.KenHaki == true then
                            KenHaki()  
                        end
                    end)
            end)
        end)


    function UnClip()
    if game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyClip") then
        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyClip"):Destroy()
    end
end


function TPGAYIsland()
    local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
    if game:GetService("Workspace").Island:FindFirstChild("Legacy Island1") then
       plr.CFrame = game:GetService("Workspace").Island:FindFirstChild("Legacy Island1").ChestSpawner.CFrame * CFrame.new(0,100,0)
    elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island2") then
       plr.CFrame = game:GetService("Workspace").Island:FindFirstChild("Legacy Island2").ChestSpawner.CFrame * CFrame.new(0,100,0)
    elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island3") then
       plr.CFrame = game:GetService("Workspace").Island:FindFirstChild("Legacy Island3").ChestSpawner.CFrame * CFrame.new(0,100,0)
    elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island4") then
       plr.CFrame = game:GetService("Workspace").Island:FindFirstChild("Legacy Island4").ChestSpawner.CFrame * CFrame.new(0,100,0)
   end
end

function FindGayIsland()
    for i, v in pairs(game.Workspace.Island:GetChildren()) do
    if string.find(v.Name, "Legacy Island") and v:IsA("Model") and v:FindFirstChild("Main") then
        wait(.8)
        UI:Notify("Impact Hub", "Legacy Island Is Up From The Sea!")
        print("Legayc Island")
        _G.SoundCheck = true
        end
    end
end
function FindHDIsland()
    for i, v in pairs(game.Workspace.Island:GetChildren()) do
    if string.find(v.Name, "Sea King") and v:IsA("Model") and v:FindFirstChild("Main") then
        wait(.8) 
        UI:Notify("Impact Hub", "Hydra Island Is Up From The Sea!")
        print("Hydra Island")
        _G.SoundCheck = true
        end
    end
end

function TPHDIsland()
    local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
    if game:GetService("Workspace").Island:FindFirstChild("Sea King Water") then
        plr.CFrame = game:GetService("Workspace").Island["Sea King Water"].Main.CFrame * CFrame.new(0,120,0)
    elseif game:GetService("Workspace").Island:FindFirstChild("Sea King Thunder") then
       plr.CFrame = game:GetService("Workspace").Island["Sea King Thunder"].Main.CFrame * CFrame.new(0,120,0)
    elseif game:GetService("Workspace").Island:FindFirstChild("Sea King Lava") then
        plr.CFrame = game:GetService("Workspace").Island["Sea King Lava"].Main.CFrame * CFrame.new(0,120,0)
    end
end

function FindGS()
        if game:GetService("Workspace").GhostMonster:FindFirstChild("Ghost Ship") then
            wait(.8)
            UI:Notify("Impact Hub", "Ghost Ship Spawned!")
            print("Ghost Ship")
            _G.SoundCheck = true
        end
    end

    function FindRaidBoss()
        if game:GetService("ReplicatedStorage").MOB:FindFirstChild("Dragon [Lv. 5000]") then
            _G.SoundCheck = true
            wait(.8)
           UI:Notify("Impact Hub", "Dragon Spawned!")
         elseif game:GetService("Workspace").Monster.Boss:FindFirstChild("Dragon [Lv. 5000]") then
            _G.SoundCheck = true
             UI:Notify("Impact Hub", "Dragon Spawned!")
            else
                _G.SoundCheck = false
            
        if game:GetService("ReplicatedStorage").MOB:FindFirstChild("Ms. Mother [Lv. 7500]") then
            _G.SoundCheck = true
           wait(.8)
          UI:Notify("Impact Hub", "Mother Spawned!")
        elseif game:GetService("Workspace").Monster.Boss:FindFirstChild("Ms. Mother [Lv. 7500]") then
            _G.SoundCheck = true
            UI:Notify("Impact Hub", "Mother Spawned!")
        else
            _G.SoundCheck = false

------------------------------------------------------------------- King Samurai ---------------------

            if game:GetService("ReplicatedStorage").MOB:FindFirstChild("King Samurai [Lv. 3500]") then
                _G.SoundCheck = true
                wait(.8)
               UI:Notify("Impact Hub", "King Samurai Spawned!")
             elseif game:GetService("Workspace").Monster.Boss:FindFirstChild("King Samurai [Lv. 3500]") then
                 UI:Notify("Impact Hub", "King Samurai Spawned!")
                 _G.SoundCheck = true
                else
                    _G.SoundCheck = false
             end
            end
        end
    end

UI:Notify("Impact Hub", "Welcom To Impact Hub.")
wait(3)
UI:Notify("Impact Hub", "Premium End At 2 January 2023")
wait(2)
FindHDIsland()
FindGayIsland()
FindGS()
FindRaidBoss()



