or MainWeaponName == "Adventure Knife" or RaidWeaponName == "Adventure Knife" or BountyWeaponName == "Adventure Knife" or MainWeaponName == "Pipe" or RaidWeaponName == "Pipe" or BountyWeaponName == "Pipe" then
