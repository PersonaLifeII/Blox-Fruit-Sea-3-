local Venyx = loadstring(game:HttpGet("https://raw.githubusercontent.com/OptusN1/VENYX-UI-2/main/VENYX%20UI%20%3D-%3D"))()
local UI  = Venyx.new("Impact Hub | Premium | Revamp Script | "..os.date("%d".." ".."%B".." ".."%Y"), 5013109572) -- date

local page1 = UI:addPage("Main Tab", 7040391851)
local FARMS = page1:addSection("Auto Farm Setting")


spawn(function()
while wait(.1) do
    if AutoFarmType == "[Default : Above] Auto Farm Type : Above" then
        Farm_Mode = CFrame.new(0,DisFarm,-1) * CFrame.Angles(math.rad(-90),0,0)
    elseif AutoFarmType == "[Default : Above] Auto Farm Type : Bellow" then
        Farm_Mode = CFrame.new(0,DisFarm,1) * CFrame.Angles(math.rad(90),0,0)
    elseif AutoFarmType == "[Default : Above] Auto Farm Type : Behind" then
        Farm_Mode = CFrame.new(0,0,DisFarm) * CFrame.Angles(math.rad(0),0,0)
    end
end
end)

AutoFarmType = "[Default : Above] Auto Farm Type : Bellow"

FarmTable = {
"[Default : Above] Auto Farm Type : Above",
"[Default : Above] Auto Farm Type : Bellow",
"[Default : Above] Auto Farm Type : Behind"
}

FARMS:addDropdown(""..AutoFarmType, FarmTable, function(a)
    AutoFarmType = a
end)

MainWeaponList = {}

for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do  
    if v:IsA("Tool") then
        table.insert(MainWeaponList, v.Name)
    end
end

FARMS:addDropdown("Select Weapon : nil", MainWeaponList,function(a)
    MainWeaponName = a
end)

DisFarm = 13
FARMS:addSlider("Distance", 13,-50,200, function(a)
    DisFarm = a
end)
FARMS:addButton("Refresh Weapon", function(a)
    table.clear(MainWeaponList)
for i,v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
    if v:IsA("Tool") then
   table.insert(MainWeaponList,v.Name)
end
end
end)

local FARM = page1:addSection("Auto Farm")

FARM:addToggle("Auto Farm Level", nil, function(value)
    _G.LvFarm = value
if value == false then
UnClip()
end
if AutoFarmType == "[Default : Above] Auto Farm Type : Above" then
UI:Notify("Impact Hub" ,"This function is only can using on Behind and Bellow type. Sorry")
end
if MainWeaponName == nil then
UI:Notify("Impact Hub", "Please Select Your Weapon") 
end
end)

task.spawn(function()
    while task.wait() do
 if _G.LvFarm then
    pcall(function()
    if game:GetService("Players").LocalPlayer.PlayerGui.Quest.QuestBoard.Visible == false then
          CheckQuest()
          if not game:GetService("Workspace").AllNPC:FindFirstChild("QuestLvl"..levelquest) then
             game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("ReplicatedStorage").MAP["QuestLvl"..levelquest].HumanoidRootPart.CFrame * CFrame.new(0,-4,0)
          else 
             game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").AllNPC["QuestLvl"..levelquest].HumanoidRootPart.CFrame * CFrame.new(0,-4,0)
          end

          wait()
          game:GetService'VirtualUser':Button1Down(Vector2.new(0,0.8))
          game:GetService'VirtualUser':Button1Up(Vector2.new(0,0.8))

          for i, v in pairs(game.Players.LocalPlayer.PlayerGui:GetDescendants()) do
             if v.Name == "Dialogue" then
                v.Accept.Size = UDim2.new(0, 10000, 0, 10000)
                v.Accept.Position = UDim2.new(-2, 0, -5, 0)
                v.Accept.ImageTransparency = 1
             end
          end
       elseif game:GetService("Players").LocalPlayer.PlayerGui.Quest.QuestBoard.Visible == true then
        pcall(function()
                    CheckQuest()
                    for i, v in pairs(game:GetService("Workspace").Monster:GetDescendants()) do
                        if v.Name == Ms then
                            repeat wait()
                                if game:GetService("Players").LocalPlayer.PlayerGui.Quest.QuestBoard.Visible == true then
                                if v.Humanoid.Health > 0 then
                                      if not game:GetService("Workspace").AllNPC:FindFirstChild("QuestLvl"..levelquest) then
                                         game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("ReplicatedStorage").MAP["QuestLvl"..levelquest].HumanoidRootPart.CFrame * CFrame.new(0,-4,0)
                                      else 
                                         game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").AllNPC["QuestLvl"..levelquest].HumanoidRootPart.CFrame * CFrame.new(0,-4,0)
                                      end
                                  for i, v in pairs(game.Players.LocalPlayer.PlayerGui:GetDescendants()) do
                                     if v.Name == "Dialogue" then
                                        v.Accept.Size = UDim2.new(0, 10000, 0, 10000)
                                        v.Accept.Position = UDim2.new(-2, 0, -5, 0)
                                        v.Accept.ImageTransparency = 1
                                     end
                                  end
                                   game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
                                   EquipWeapon1(MainWeaponName)
                                   Haki()
                                   end
                                else
                                     CheckQuest()
                                  if not game:GetService("Workspace").AllNPC:FindFirstChild("QuestLvl"..levelquest) then
                                     game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("ReplicatedStorage").MAP["QuestLvl"..levelquest].HumanoidRootPart.CFrame * CFrame.new(0,-4,0)
                                  else 
                                     game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").AllNPC["QuestLvl"..levelquest].HumanoidRootPart.CFrame * CFrame.new(0,-4,0)
                                  end
                                  game:GetService'VirtualUser':Button1Down(Vector2.new(0,0.8))
                                game:GetService'VirtualUser':Button1Up(Vector2.new(0,0.8))
                                  wait()
                                  for i, v in pairs(game.Players.LocalPlayer.PlayerGui:GetDescendants()) do
                                     if v.Name == "Dialogue" then
                                        v.Accept.Size = UDim2.new(0, 10000, 0, 10000)
                                        v.Accept.Position = UDim2.new(-2, 0, -5, 0)
                                        v.Accept.ImageTransparency = 1
                                     end
                                  end
                                end
                            until not v.Parent or v.Humanoid.Health <= 0 or _G.LvFarm == false
                        elseif v.Name ~= Ms then
                            if not game:GetService("Workspace").AllNPC:FindFirstChild("QuestLvl"..levelquest) then
                                     game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("ReplicatedStorage").MAP["QuestLvl"..levelquest].HumanoidRootPart.CFrame * CFrame.new(0,50,0)
                                  else 
                                     game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").AllNPC["QuestLvl"..levelquest].HumanoidRootPart.CFrame * CFrame.new(0,50,0)
                                  end
                                end
                        end
                            end)
                        end
                            end)
                        end
                end
            end)


local mobs = {} 
getgenv().mob = nil 

for _,v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do
    insert = true 
    for _,v2 in pairs(mobs) do if v2 == v.Name then insert = false end end 
    if insert then table.insert(mobs, v.Name) end 
end

local mobdropdown = FARM:addDropdown("Select Mon", mobs, function(v)
    getgenv().mob = v
end)

FARM:addToggle("Auto Farm Mon", nil, function(v)
    getgenv().autofarmmobs = v
    while wait() do
        if getgenv().autofarmmobs == false then return end 
        if getgenv().mob == nil then 
            game.StarterGui:SetCore("SendNotification", {
                Title = "Impact Hub", 
                Text = "Please Select your Mon",
                Icon = "",
                Duration = 2.5
            })
            getgenv().autofarmmobs = false
            return
        end
        local mob = game:GetService("Workspace").Monster.Mon:FindFirstChild(getgenv().mob)
        if mob == nil then
            UI:Notify("There Are No Spawned Mons Of This Type\n Please Wait Until They Spawn")
            while wait() do 
                wait() 
                if getgenv().autofarmmobs == false then return end 
                if game:GetService("Workspace").Monster.Mon:FindFirstChild(getgenv().mob) ~= nil then break; end
            end 
        else
            local mob2 = mob
            while wait() do
                mob = game:GetService("Workspace").Monster.Mon:FindFirstChild(getgenv().mob)
                if mob ~= mob2 then break; end
                if getgenv().autofarmmobs == false then return end
                if mob ~= nil then
                    if mob:FindFirstChild("Humanoid") then
                        if mob.Humanoid.Health == 0 then wait(0.1) mob:Destroy() break; end 
                    end
                    if mob:FindFirstChild("HumanoidRootPart") then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = mob.HumanoidRootPart.CFrame * Farm_Mode
                        EquipWeapon1(MainWeaponName)
                        Haki()
                    end
                end
                wait() 
            end
        end
    end
end)

game:GetService("Workspace").Monster.Mon.ChildAdded:Connect(function() 
    for _,v2 in pairs(mobs) do table.remove(mobs, _) end
    
    for _,v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do
        insert = true 
        for _,v2 in pairs(mobs) do if v2 == v.Name then insert = false end end
        if insert then table.insert(mobs, v.Name) end 
    end
    mobdropdown:Refresh(mobs)
end)

game:GetService("Workspace").Monster.Mon.ChildRemoved:Connect(function() 
    for _,v2 in pairs(mobs) do table.remove(mobs, _) end 
    
    for _,v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do 
        insert = true 
        for _,v2 in pairs(mobs) do if v2 == v.Name then insert = false end end 
        if insert then table.insert(mobs, v.Name) end 
    end
    mobdropdown:Refresh(mobs)
end)
    
local GameTime = math.floor(workspace.DistributedGameTime+0.5)
local Hour = math.floor(GameTime/(60^2))%24
local Minute = math.floor(GameTime/(60^1))%60
local Second = math.floor(GameTime/(60^0))%60
-------------------------------- Nothing
local CoreGui = game:GetService("StarterGui")
    FARM:addButton("Check Server Time", function()
         UI:Notify("Impact Hub", "Total Time You Have Joined\nHour : "..Hour.." Minute : "..Minute.." Second : "..Second);
    end)
    FARM:addButton("Teleport To Current Quest", function()
        CheckQuest()
        if not game:GetService("Workspace").AllNPC:FindFirstChild("QuestLvl"..levelquest) then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("ReplicatedStorage").MAP["QuestLvl"..levelquest].HumanoidRootPart.CFrame
         else 
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").AllNPC["QuestLvl"..levelquest].HumanoidRootPart.CFrame
         end
    end)
    _G.Buso = true
    FARM:addToggle("Auto Armament Haki",  true, function(a)
        _G.Buso = a
            end)
    FARM:addToggle("Auto Observation Haki",  nil, function(a)
        _G.KenHaki = a
    end)

local FARM_NOTIFY = page1:addSection("Boss Noification")

_G.BossNotify = true
FARM_NOTIFY:addToggle("Boss Notify", true, function(value)
    _G.BossNotify = value
end)

task.spawn(function()
    while task.wait() do
        if _G.BossNotify then
        pcall(function()
                 FindGayIsland()
               FindHDIsland()
              FindGS()
            FindRaidBoss()
        end)
     end
    end
end)

FARM_NOTIFY:addToggle("Boss Notify Sound", nil, function(value)
    _G.BossNotify_SoundAdd = value
    if value == false then
        _G.SoundCheck = false
    end
end)

task.spawn(function()
while task.wait() do
if _G.BossNotify_SoundAdd and _G.BossNotify then
pcall(function()
if _G.SoundCheck == true then
if not game:GetService("Workspace"):FindFirstChild("Boss_Notify") then
a = Instance.new("Sound", game:GetService("Workspace"))
a.Name = "Boss_Notify"
a.Volume = _G.BossNotify_SoundAdd_Volume
a.Looped = false
a.SoundId = "rbxassetid://1523818134"
a.Playing = _G.BossNotify_SoundAdd
end
end
end)
end
end
end)

_G.BossNotify_SoundAdd_Volume = 3
FARM_NOTIFY:addSlider("Boss Notify",  3, 1, 30, function(value)
    _G.BossNotify_SoundAdd_Volume = value
end)




local FARM2 = page1:addSection("Sea King")

FARM2:addToggle("Auto Farm Sea King", nil, function(value)
    _G.SeaKing = value
    if value == true then
    if AutoFarmType == "[Default : Above] Auto Farm Type : Bellow" or AutoFarmType == "[Default : Above] Auto Farm Type : Above" then
        UI:Notify("Impact Hub", "This Function Is Can Be Using In Behind Type Only")
    end
    if game:GetService("Workspace").SeaMonster:FindFirstChild("SeaKing") then
    UI:Notify("Impact Hub", "Recommended For Distance 200")
    end
    elseif value == false then
        UnClip()
      end
end)

task.spawn(function()
    while task.wait() do
        if _G.SeaKing then
        pcall(function()
            TPGAYIsland()
            local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
            for i,v in pairs(game:GetService("Workspace").SeaMonster:GetDescendants()) do
                if game:GetService("Workspace").SeaMonster:FindFirstChild("SeaKing") then
                   if v.Name == "SeaKing" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                    plr.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,DisFarm)
                    EquipWeapon1(MainWeaponName)
                    Haki()
                end
            end
        end
    end)
     end
    end
end)
FARM2:addToggle("Auto Grab Chest", nil, function(a)
    _G.GrabChest = a

    task.spawn(function()
        while task.wait() do
            if not _G.GrabChest then break end
            pcall(function()
                if game:GetService("Workspace").SeaMonster:FindFirstChild("SeaKing") then
                for i, v in pairs(game.Workspace.SeaMonster:GetChildren()) do
                if v.Name == "SeaKing" and v:FindFirstChild("Humanoid") then
                    if v.Humanoid.Health <= 0 then
                    if game:GetService("Workspace").Island:FindFirstChild("Legacy Island1") then
                       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Island:FindFirstChild("Legacy Island1").ChestSpawner.CFrame
                    elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island2") then
                       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Island:FindFirstChild("Legacy Island2").ChestSpawner.CFrame
                    elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island3") then
                       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Island:FindFirstChild("Legacy Island3").ChestSpawner.CFrame
                    elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island4") then
                       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Island:FindFirstChild("Legacy Island4").ChestSpawner.CFrame
                        end
                      end
                    end
                end
            end
        end)
     end
    end)
end)
FARM2:addButton("Teleport To Legacy Island", function(a)    
  TPGAYIsland()
end)

local FARM4 = page1:addSection("Dragon")

FARM4:addToggle("Auto Dragon Gem", nil, function(value)
    _G.Orbs = value
    if value == false then
        UnClip()
    end
end)
    
    task.spawn(function()
        while task.wait() do
            if _G.Orbs == true then
            pcall(function()
                local plr = game.Players.LocalPlayer.Character.HumanoidRootPart

                if not game.Players.LocalPlayer.Backpack:FindFirstChild("DragonGem") then
                plr.CFrame = CFrame.new(-6122, 165, 7168)

                for i, v in pairs(game.Workspace.Monster.Boss:GetChildren()) do
                    if v.Name == "Elite Skeleton [Lv. 3100]" and v:FindFirstChild("Humanoid") then
                        plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
                        EquipWeapon1(MainWeaponName)
                        Haki()
                    if v.Humanoid.Health <= 0 then
                        plr.CFrame = CFrame.new(-6122, 165, 7168)
                        wait(5)
                        print("DEAD")
                            end
                          end
                        end
                    end
                end)
            end
            end
        end)

