local Raid = page5:addSection("Raid")
           ----------      ?  "if newworld then       :>"

Raid:addToggle("Auto Kill All Monster", nil, function(value)
    _G.KillRaidMon = value
    if value == false then
        UnClip()
    end
end)

task.spawn(function()
    while task.wait() do
        if _G.KillRaidMon then
        pcall(function()
            local plr = game.Players.localPlayer.Character.HumanoidRootPart

            for i, v in pairs(game:GetService("Workspace").MOB:GetChildren()) do
                if v:IsA("Model") and v:FindFirstChild("Humanoid") then
                if v.Humanoid.Health > 0 then
                MinHealth = game.Players.LocalPlayer.Character.Humanoid.MaxHealth * 50 / 100
                if game.Players.LocalPlayer.Character.Humanoid.Health > MinHealth then
                    plr.CFrame = v.HumanoidRootPart.CFrame * Raid_Mode
                    EquipWeapon2(RaidWeaponName)
                    Haki()
                        end
                    end
                end
            end
            MinHealth = game.Players.LocalPlayer.Character.Humanoid.MaxHealth * 50 / 100
            if game.Players.LocalPlayer.Character.Humanoid.Health < MinHealth then
                plr.CFrame = CFrame.new(0,1000,0)
            end 
        end)
    end
end
end)


