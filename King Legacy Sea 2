        local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
        if not game:GetService("ReplicatedStorage").MOB:FindFirstChild("Dark Beard [Lv. 3475]") then
    plr.CFrame = CFrame.new(-9490, 186, -4904)
        for i,v in pairs(game.Workspace.Monster.Mon:GetChildren()) do
            if v.Name == "Dark Beard Servant [Lv. 3400]" and v:IsA("Model") and v:FindFirstChild("Humanoid") then
            if v.Humanoid.Health > 0 then
                plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
                EquipWeapon1(MainWeaponName)
                Haki()
            elseif v.Humanoid.Health <= 0 then
            plr.CFrame = CFrame.new(-9740, 61, -5018)
            wait()
            game:GetService'VirtualUser':Button1Down(Vector2.new(0,0.8))
            game:GetService'VirtualUser':Button1Up(Vector2.new(0,0.8))
             for i, v in pairs(game.Players.LocalPlayer.PlayerGui:GetDescendants()) do
                if v.Name == "Dialogue" then
                   v.Accept.Size = UDim2.new(0, 10000, 0, 10000)
                   v.Accept.Position = UDim2.new(-2, 0, -5, 0)
                   v.Accept.ImageTransparency = 1
                elseif game:GetService("ReplicatedStorage").MOB:FindFirstChild("Dark Beard [Lv. 3475]") then
        plr.CFrame = CFrame.new(-10160, 145, -5140)
        for i, v in pairs(game.Workspace.Monster.Boss:GetChildren()) do
        if v.Name == "Dark Beard [Lv. 3475]" and v:FindFirstChild("Humanoid") then
        if v.Humanoid.Health > 0 then
        plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
        EquipWeapon1(MainWeaponName)
        Haki()
    end
    end
end
end
end
end
end
end
end
end)
end
end
