                                         pcall(function()
                                            game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed += 20
                                        end)
                                         end)
                                        end)

                                        game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed += 20


