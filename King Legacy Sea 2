local FARM4 = page1:addSection("Dragon")


FARM4:addToggle("Auto Farm Dragon", nil, function(value)
    _G.Dragon = value
if value == false then
        UnClip()
elseif value == true then
    if AutoFarmType == "[Default : Above] Auto Farm Type : Bellow" or "[Default : Above] Auto Farm Type : Above" then
         UI:Notify("Impact Hub", "This Function Is Can Be Using In Behind Type Only")
      end
   end
   if MainWeaponName == nil then
    UI:Notify("Impact Hub", "Please Select Your Weapon") 
    end
end)

    task.spawn(function()
        while task.wait() do
            if _G.Dragon then
            pcall(function()
                local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
                if not game:GetService("Workspace").Monster.Boss:FindFirstChild("Dragon [Lv. 5000]") then
                    plr.CFrame = game:GetService("ReplicatedStorage").MOB["Dragon [Lv. 5000]"].HumanoidRootPart.CFrame * CFrame.new(0,80,0)
                end

                for i, v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
                        if v.Name == "Dragon [Lv. 5000]" and v:FindFirstChild("Humanoid") then
                            if v.Humanoid.Health > 0 then
                            plr.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,DisFarm)
                            EquipWeapon1(MainWeaponName)
                            Haki()
                            elseif v.Humanoid.Health >= 0 then
                                plr.CFrame = CFrame.new(-6025, 442, 7444)
                            end
                        end
                    end
                    if _G.Farm_Lv then
                        if not game:GetService("ReplicatedStorage").MOB:FindFirstChild("Dragon [Lv. 5000]") then
                        wait(3)
                        _G.Farm_Lv = true
                        else
                            _G.Farm_Lv = false
                        end
                    end
                    end)
            end
        end
    end)

        FARM4:addToggle("Auto Dragon Gem", nil, function(value)
            _G.Orbs = value
            if value == false then
                UnClip()
            end
            if MainWeaponName == nil then
                UI:Notify("Impact Hub", "Please Select Your Weapon") 
                end
        end)
            
            task.spawn(function()
                while task.wait() do
                    if _G.Orbs == true then
                    pcall(function()
                        local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
        
            if not game:GetService("ReplicatedStorage").MOB:FindFirstChild("Dragon [Lv. 5000]") then
                        if not game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("DragonGem") then
                        plr.CFrame = CFrame.new(-6122, 165, 7168)
        
                        for i, v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
                            if v.Name == "Elite Skeleton [Lv. 3100]" and v:FindFirstChild("Humanoid") then
                                if v.Humanoid.Health > 0 then
                                plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
                                EquipWeapon1(MainWeaponName) 
                                Haki()
                            elseif v.Humanoid.Health <= 0 then
                                plr.CFrame = CFrame.new(-6122, 165, 7168)
                                        end
                                    end
                                  end
                                end
                            end
                        end)
                    end
                    end
                end)
        
                FARM4:addToggle("Auto Dragon Gem [QUEST]", nil, function(value)
            _G.OrbsQ = value
        if value == false then
                CheckQuest()
                UnClip()
        end
        if value == true then

        CheckQuest()
        if MyLevel <= 3075 then
            UI:Notify("Impact Hub" ,"Level 3075 Require")
        end
        if MainWeaponName == nil then
            UI:Notify("Impact Hub", "Please Select Your Weapon") 
            end
        end
        end)
    
        task.spawn(function()
            while task.wait() do
         if _G.OrbsQ then
            pcall(function()
            local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
        
            EquipWeapon1(MainWeaponName)
            Haki()
        
            if game:GetService("Players").LocalPlayer.PlayerGui.Quest.QuestBoard.Visible == false then
                if game:GetService("Workspace").AllNPC:FindFirstChild("QuestLvl3075") then
                    plr.CFrame = game:GetService("Workspace").AllNPC["QuestLvl3075"].HumanoidRootPart.CFrame * CFrame.new(0,-5,0)
                    wait()

                    UIClick()
                else 
                    plr.CFrame = game:GetService("ReplicatedStorage").MAP["QuestLvl3075"].HumanoidRootPart.CFrame * CFrame.new(0,-5,0)
                    wait()

                    UIClick()
                 end    
        
        
                 CheckQuest()
                  if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("QuestLvl3075") then
                    if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("QuestLvl3075").Dialogue then
                        if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("QuestLvl3075").Dialogue.Accept then
        
                            game:GetService("Players").LocalPlayer.PlayerGui["QuestLvl3075"].Dialogue.Accept.Size = UDim2.new(0, 10000, 0, 10000)
                            game:GetService("Players").LocalPlayer.PlayerGui["QuestLvl3075"].Dialogue.Accept.Position = UDim2.new(-2, 0, -5, 0)
                            game:GetService("Players").LocalPlayer.PlayerGui["QuestLvl3075"].Dialogue.Accept.ImageTransparency = 1
                    end
                end
            end
        end

        if not game:GetService("Workspace").Monster.Boss:FindFirstChild("Elite Skeleton [Lv. 3100]") then
            plr.CFrame = CFrame.new(-6122, 165, 7168)
        end

        if game:GetService("Players").LocalPlayer.PlayerGui.Quest.QuestBoard.Visible == true then
            CheckQuest()
        if game:GetService("Workspace").Monster.Mon:FindFirstChild("Elite Skeleton [Lv. 3100]") then
            for i, v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do
              if v.Name == "Elite Skeleton [Lv. 3100]" then
                if v.Humanoid.Health > 0 then
                plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
            elseif v.Humanoid.Health >= 0 then
                v:Destroy() break;
                end
            end
        end
            CheckQuest()
            elseif game:GetService("Workspace").Monster.Boss:FindFirstChild("Elite Skeleton [Lv. 3100]") then
                    for i, v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
                      if v.Name == "Elite Skeleton [Lv. 3100]" then
                        if v.Humanoid.Health > 0 then
                        plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
                        elseif v.Humanoid.Health <= 0 then

                            plr.CFrame = CFrame.new(-6080, 178, 7179)
                            v:Destroy() break;
                        end
                    end
                end  
            end
        end
        end)
        end
        end
        end)



