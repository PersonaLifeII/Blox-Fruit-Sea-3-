        plr.CFrame = game:GetService("ReplicatedStorage").MOB["Dragon [Lv. 5000]"].HumanoidRootPart.CFrame * CFrame.new(0,100,0)
        print("Replicated Storage")
    elseif game:GetService("Workspace").Monster.Boss:FindFirstChild("Dragon [Lv. 5000]") then

        plr.CFrame = game:GetService("Workspace").Monster.Boss["Dragon [Lv. 5000]"].HumanoidRootPart.CFrame * CFrame.new(0,100,0)
        print("Workspace")
    end
end)
    

local FARM5 = page1:addSection("Mother")

FARM5:addToggle("Auto Farm Ms. Mother", nil, function(value)
    _G.Mother = value
    if value == false then
        UnClip()
    end
end)

task.spawn(function()
    while task.wait() do
        if _G.Mother == true then
        pcall(function()
            local plr = game.Players.LocalPlayer.Character.HumanoidRootPart

            if not game:GetService("Workspace").Monster.Boss:FindFirstChild("Ms. Mother [Lv. 7500]") then
                plr.CFrame = game:GetService("ReplicatedStorage").MOB["Ms. Mother [Lv. 7500]"].HumanoidRootPart.CFrame * CFrame.new(0,80,0)
            end

            for i, v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
                    if v.Name == "Ms. Mother [Lv. 7500]" and v:FindFirstChild("Humanoid") then
                        if v.Humanoid.Health > 0 then
                        plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
                        EquipWeapon1(MainWeaponName)
                        Haki()
                        end
                    end
                end
                if _G.Farm_Lv then
                    if not game:GetService("ReplicatedStorage").MOB:FindFirstChild("Ms. Mother [Lv. 7500]") then
                    wait(3)
                    _G.Farm_Lv = true
                    else
                        _G.Farm_Lv = false
                    end
                end
            end)
        end
    end
end)


FARM5:addButton("Teleport To Ms. Mother", function(a)
    local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
    if game:GetService("ReplicatedStorage").MOB:FindFirstChild("Ms. Mother [Lv. 7500]") then

        plr.CFrame = game:GetService("ReplicatedStorage").MOB["Ms. Mother [Lv. 7500]"].HumanoidRootPart.CFrame * CFrame.new(0,80,0)
    elseif game:GetService("Workspace").Monster.Boss:FindFirstChild("Ms. Mother [Lv. 7500]") then
        
        plr.CFrame = game:GetService("Workspace").Monster.Boss["Ms. Mother [Lv. 7500]"].HumanoidRootPart.CFrame * CFrame.new(0,100,0)
    end
end)

local FARM5 = page1:addSection("King Samurai")

FARM5:addToggle("Auto Farm King Samurai", nil, function(value)
    _G.Hell = value
if value == false then
        UnClip()
    end
end)

task.spawn(function()
    while task.wait() do
        if _G.Hell == true then
        pcall(function()
            local plr = game.Players.LocalPlayer.Character.HumanoidRootPart

            if not game:GetService("Workspace").Monster.Boss:FindFirstChild("King Samurai [Lv. 3500]") then
                plr.CFrame = game:GetService("ReplicatedStorage").MOB["King Samurai [Lv. 3500]"].HumanoidRootPart.CFrame * CFrame.new(0,30,50)
            end

            for i, v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
                    if v.Name == "King Samurai [Lv. 3500]" and v:FindFirstChild("Humanoid") then
                        if v.Humanoid.Health > 0 then
                        plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
                        EquipWeapon1(MainWeaponName)
                        Haki()
                        end
                    end
                end
                if _G.Farm_Lv then
                    if not game:GetService("ReplicatedStorage").MOB:FindFirstChild("King Samurai [Lv. 3500]") then
                    wait(3)
                    _G.Farm_Lv = true
                    else
                        _G.Farm_Lv = false
                    end
                end
            end)
        end
    end
end)


FARM5:addButton("Teleport To King Samurai", function(a)
    local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
    if game:GetService("ReplicatedStorage").MOB:FindFirstChild("King Samurai [Lv. 3500]")  then
    
        plr.CFrame = game:GetService("ReplicatedStorage").MOB["King Samurai [Lv. 3500]"].HumanoidRootPart.CFrame * CFrame.new(0,60,0)
        print("Replicated")
    elseif game:GetService("Workspace").Monster.Boss:FindFirstChild("King Samurai [Lv. 3500]") then
        
        plr.CFrame = game:GetService("Workspace").Monster.Boss["King Samurai [Lv. 3500]"].HumanoidRootPart.CFrame * CFrame.new(0,60,0)
        print("Workspace")
    end
end)

local FARM7 = page1:addSection("Ghost Ship")

FARM7:addToggle("Auto Farm Ghost Ship", nil, function(value)
    _G.Ghost = value
    if value == false then
        UnClip()
    end
    if value == true then
    if not game:GetService("Workspace").GhostMonster:FindFirstChild("Ghost Ship") then
    UI:Notify("Impact Hub", "There Are No Spawned Ghost Ship")
    end
    if AutoFarmType == "[Default : Above] Auto Farm Type : Bellow" or "[Default : Above] Auto Farm Type : Above" then
    if game:GetService("Workspace").GhostMonster:FindFirstChild("Ghost Ship") then
        wait(3)
        UI:Notify("Impact Hub", "This Function Is Can Be Using In Behind Type Only")
        end
    end
end
end)
    
    task.spawn(function()
        while task.wait() do
            if _G.Ghost == true then
                pcall(function()
                local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
            if game:GetService("Workspace").GhostMonster:FindFirstChild("Ghost Ship") then
                    for i, v in pairs(game.Workspace.GhostMonster:GetChildren()) do
                    if v.Name == "Ghost Ship" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                        plr.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,10,DisFarm)
                        EquipWeapon1(MainWeaponName)
                        Haki()
                       end
                    end
                end
                if _G.Farm_Lv then
                    if not game:GetService("Workspace").GhostMonster:FindFirstChild("Ghost Ship") then
                    wait(3)
                    _G.Farm_Lv = true
                    else
                        _G.Farm_Lv = false
                    end
                end
            end)
        end
    end
    end)

FARM7:addToggle("Auto Grab Chest", nil, function(value)
    _G.GrabGhostChest = value
if value == false then
        UnClip()
    end
end)


    task.spawn(function()
        while task.wait() do
            if _G.GrabGhostChest then 
            pcall(function()
            local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
                 
            if game:GetService("Workspace"):FindFirstChild("Chest1") then
                plr.CFrame = game:GetService("Workspace")["Chest1"].RootPart.CFrame
            end
            if game:GetService("Workspace"):FindFirstChild("Chest2") then
                plr.CFrame = game:GetService("Workspace")["Chest2"].RootPart.CFrame
            end
            if game:GetService("Workspace"):FindFirstChild("Chest3") then
                plr.CFrame = game:GetService("Workspace")["Chest3"].RootPart.CFrame
            end
            if game:GetService("Workspace"):FindFirstChild("Chest4") then
                plr.CFrame = game:GetService("Workspace")["Chest4"].RootPart.CFrame
            end
            if game:GetService("Workspace"):FindFirstChild("Chest5") then
                plr.CFrame = game:GetService("Workspace")["Chest5"].RootPart.CFrame
    end
end)
end
end
end)

