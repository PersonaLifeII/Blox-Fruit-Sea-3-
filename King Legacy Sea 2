DFSelected = "BombBomb"
DevilFruit:addDropdown("Select Devil Fruit : "..DFSelected, DevilFruitList, function(value)
    DFSelected = value
    print("Devil Fruit Select Status : "..DFSelected)
end)
DevilFruit:addToggle("Auto Buy Devil Fruit", nil, function(value)
    _G.DFSnipe = value
end)

task.spawn(function()
    while task.wait() do
        if _G.DFSnipe == true then
        pcall(function()
        if not game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(DFSelected) then
            local args = 
            {
                [1] = DFSelected,
                [2] = true
            }         
                game:GetService("ReplicatedStorage").Remotes.Functions.dfbeli:InvokeServer(unpack(args))
        else
            error("Devil Fruit Are Not In Stock or You Already Own This Devil Fruit")
        end
    end)
     end
    end
end)


 
RandomDFTable = {"Random Devil Fruit Type : Beli", "Random Devil Fruit Type : Gem"}
RandomDFType = "Random Devil Fruit Type : Beli"
DevilFruit:addDropdown(""..RandomDFType, RandomDFTable, function(value)
    RandomDFType = value
end)
DevilFruit:addToggle("Auto Random Devil Fruit", nil, function(value)
    _G.RandomDF1 = value
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-4910, 57, 113)
end)

    task.spawn(function()
        while task.wait() do
            if _G.RandomDF1 == true then
            pcall(function()
                local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
                for i, v in pairs(game.Workspace.AllNPC:GetChildren()) do
                    if v.Name == "ARandomFruit" and v:FindFirstChild("FAKEHumanoid") then
                        plr.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,-5,0)
                        game:GetService'VirtualUser':Button1Down(Vector2.new(0,0.8))
                        game:GetService'VirtualUser':Button1Up(Vector2.new(0,0.8))
                        if RandomDFType == "Random Devil Fruit Type : Beli" then
                        for i, v in pairs(game.Players.LocalPlayer.PlayerGui:GetDescendants()) do
                            if v.Name == "Dialogue" then
                               v.Beli.Size = UDim2.new(0, 10000, 0, 10000)
                               v.Beli.Position = UDim2.new(-2, 0, -5, 0)
                               v.Beli.ImageTransparency = 1
                            elseif RandomDFType == "Random Devil Fruit Type : Gem" then
                                for i, v in pairs(game.Players.LocalPlayer.PlayerGui:GetDescendants()) do
                                    if v.Name == "Dialogue" then
                                       v.Gem.Size = UDim2.new(0, 10000, 0, 10000)
                                       v.Gem.Position = UDim2.new(-2, 0, -5, 0)
                                       v.Gem.ImageTransparency = 1
                            end
                        end
                         end
                    end
                end
            end
        end
    end)
        end
    end
end)

