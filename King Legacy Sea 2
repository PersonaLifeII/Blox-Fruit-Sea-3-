            if v.Name == "Chest1" and v:FindFirstChild("RootPart") and v:IsA("Model") then
                plr.CFrame = v.RootPart.CFrame
            elseif v.Name == "Chest2" and v:FindFirstChild("RootPart") and v:IsA("Model") then
            plr.CFrame = v.RootPart.CFrame
            elseif v.Name == "Chest3" and v:FindFirstChild("RootPart") and v:IsA("Model") then
             plr.CFrame = v.RootPart.CFrame
          elseif v.Name == "Chest4" and v:FindFirstChild("RootPart") and v:IsA("Model") then
                plr.CFrame = v.RootPart.CFrame
            elseif v.Name == "Chest5" and v:FindFirstChild("RootPart") and v:IsA("Model") then
                plr.CFrame = v.RootPart.CFrame
            end
        end
        end
    end
end
end
