local FARM3 = page1:addSection("Ghost Ship")

FARM3:addToggle("Auto Farm Ghost Ship", nil, function(value)
    _G.Ghost = value
    if value == false then
        UnClip()
    end
    if value == true then
    if not game:GetService("Workspace").GhostMonster:FindFirstChild("Ghost Ship") then
    UI:Notify(HubName, "There Are No Spawned Ghost Ship")
    end
    if AutoFarmType == "[Default : Above] Auto Farm Type : Bellow" or "[Default : Above] Auto Farm Type : Above" then
    if game:GetService("Workspace").GhostMonster:FindFirstChild("Ghost Ship") then
        wait(3)
        UI:Notify(HubName, "This Function Is Can Be Using In Behind Type Only")
        end
    end
end
end)
    
    task.spawn(function()
        while task.wait() do
            if _G.Ghost then
                pcall(function()
                local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
            if game:GetService("Workspace").GhostMonster:FindFirstChild("Ghost Ship") then

                    for i, v in pairs(game:GetService("Workspace").GhostMonster:GetChildren()) do
                    if v.Name == "Ghost Ship" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                        plr.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,10,DisFarm)
                        EquipWeapon1(MainWeaponName)
                        Haki()
                       end
                    end
                end
            end)
        end
    end
    end)

    FARM3:addToggle("Auto Farm Ghost Ship Hop", nil, function(value)
        _G.GsHop = value
        if value == false then
            UnClip()
        end
    end)
    
    task.spawn(function()
        while task.wait() do
            if _G.GsHop then
                pcall(function()
                if not game:GetService("Workspace").GhostMonster:FindFirstChild("Ghost Ship") then
                    UI:Notify("Impact Hub", "Hoping...")
                    wait(2)
                    Hop()
                end
            end)
        end
    end
    end)

    FARM3:addToggle("Auto Grab Chest", nil, function(value)
    _G.GrabGhostChest = value
if value == false then
        UnClip()
    end
end)


    task.spawn(function()
        while task.wait() do
            if _G.GrabGhostChest then 
            pcall(function()
            local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
                 
            if game:GetService("Workspace"):FindFirstChild("Chest1") then
                wait(0.1)
                plr.CFrame = game:GetService("Workspace")["Chest1"].RootPart.CFrame
            end
            if game:GetService("Workspace"):FindFirstChild("Chest2") then
                wait(0.2)
                plr.CFrame = game:GetService("Workspace")["Chest2"].RootPart.CFrame
            end
            if game:GetService("Workspace"):FindFirstChild("Chest3") then
                wait(0.3)
                plr.CFrame = game:GetService("Workspace")["Chest3"].RootPart.CFrame
            end
            if game:GetService("Workspace"):FindFirstChild("Chest4") then
                wait(0.4)
                plr.CFrame = game:GetService("Workspace")["Chest4"].RootPart.CFrame
            end
            if game:GetService("Workspace"):FindFirstChild("Chest5") then
                wait(0.5)
                plr.CFrame = game:GetService("Workspace")["Chest5"].RootPart.CFrame
            end
