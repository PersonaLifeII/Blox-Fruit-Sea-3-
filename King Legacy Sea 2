                        else 
                            QuestLevel = 3250
                            plr.CFrame = game:GetService("ReplicatedStorage").MAP["QuestLvl"..QuestLevel].HumanoidRootPart.CFrame * CFrame.new(0,-5,0)
                            wait()
                
