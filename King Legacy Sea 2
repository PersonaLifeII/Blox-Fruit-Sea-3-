        pcall(function()
            local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
            if game:GetService("ReplicatedStorage").MOB:FindDevilFruit("Dark Beard [Lv. 3475]") then
                if not game:GetService("Workspace").Monster.Boss:FindFirstChild("Dark Beard [Lv. 3475]") then
            plr.CFrame = CFrame.new(-10160, 145, -5140)
            end

            for i,v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
                if v.Name == "Dark Beard [Lv. 3475]" and v:IsA("Model") and v:FindFirstChild("Humanoid") then
                    if v.Humanoid.Health > 0 then
                    plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
                    EquipWeapon1(MainWeaponName)
                    Haki()
                    end
                end
            end
        end
        end)
     end
    end
end)

