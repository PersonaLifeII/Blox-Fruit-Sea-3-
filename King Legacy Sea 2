function KenHaki()
    local pl = game.Players.LocalPlayer.Character
  if pl.KenOpen.Value == false then
	game:service('VirtualInputManager'):SendKeyEvent(true, "Y", false, game)
	game:service('VirtualInputManager'):SendKeyEvent(true, "Y", false, game)
    end
end

 function Haki()
    local pl = game.Players.LocalPlayer.Character
    if not pl:FindFirstChild("ArmamentBodyGroup") then
        if pl.Haki.Value == 0 then
            game:service('VirtualInputManager'):SendKeyEvent(true, "T", false, game)
            game:service('VirtualInputManager'):SendKeyEvent(true, "T", false, game)
        end
end
end


