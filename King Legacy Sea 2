FARM5:addToggle("Auto Farm Ms. Mother", nil, function(value)
    _G.Mother = value
    if value == false then
        UnClip()
    end
end)

    task.spawn(function()
        while task.wait() do
            if _G.Mother == true then
            pcall(function()
                local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
                if game:GetService("ReplicatedStorage").MOB:FindFirstChild("Ms. Mother [Lv. 7500]") then
                    plr.CFrame = CFrame.new(-290, 191, 9272)
                wait()
                for i, v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
                        if v.Name == "Ms. Mother [Lv. 7500]" and v:FindFirstChild("Humanoid") then
                            if v.Humanoid.Health > 0 then
                            plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
                            EquipWeapon1(MainWeaponName)
                            Haki()
                        end
                    end
                        end
                    end
                end)
    end
    end
        end)

