 spawn(function()
    pcall(function()
         while wait() do
            if _G.KillRaidMon == true then
                if RaidWeaponName == "Muramasa" or RaidWeaponName == "Pheonix Blade" or RaidWeaponName == "Bisento" or RaidWeaponName == "Katana" or RaidWeaponName == "Tashi Blade" or RaidWeaponName == "Authentic Triple Katana" or RaidWeaponName == "Authentic Mace" or RaidWeaponName == "Night Blade" or RaidWeaponName == "Hell Sword" or RaidWeaponName == "Sunken Blade" or RaidWeaponName == "Kioru" or RaidWeaponName == "Jitter" or RaidWeaponName == "Anubis Axe" or RaidWeaponName == "Soul Cane" or RaidWeaponName == "Adventure Knife" or RaidWeaponName == "Metal Trident" or RaidWeaponName == "Saber"
                or RaidWeaponName == "Cookie Sword" or RaidWeaponName == "Pipe" or RaidWeaponName == "Pole" or RaidWeaponName == "Shark Blade" or RaidWeaponName == "Acroscyth" or RaidWeaponName == "Longaevus" or RaidWeaponName == "Pumpkin Smasher" or RaidWeaponName == "Gold Spear" or RaidWeaponName == "Aquatic Anchor" or RaidWeaponName == "Scepters of Flames" or RaidWeaponName == "Barbaric Axe" or RaidWeaponName == "Demon Trident" or RaidWeaponName == "Pondere Blade" or RaidWeaponName == "Pondere Blade" or RaidWeaponName == "Hunter Blade" then
                  local args = {
