local RaidS = page5:addSection("Raid Setting")
    
spawn(function()
    while wait(.1) do
        if AutoRaidType == "[Default : Above] Auto Farm Type : Above" then
            Raid_Mode = CFrame.new(0,DisRaid,-1) * CFrame.Angles(math.rad(-90),0,0)
        elseif AutoRaidType == "[Default : Above] Auto Farm Type : Bellow" then
            Raid_Mode = CFrame.new(0,DisRaid,1) * CFrame.Angles(math.rad(90),0,0)
        elseif AutoRaidType == "[Default : Above] Auto Farm Type : Behind" then
            Raid_Mode = CFrame.new(0,0,DisRaid) * CFrame.Angles(math.rad(0),0,0)
        end
    end
    end)
    
    RaidTable = {
    "[Default : Above] Auto Farm Type : Above",
    "[Default : Above] Auto Farm Type : Bellow",
    "[Default : Above] Auto Farm Type : Behind"
    }
    
    -- AutoFarmType = "[Default : Above] Auto Farm Type : Above"
    AutoRaidType = RaidTable[1]
    
    RaidS:addDropdown(""..AutoRaidType, RaidTable, function(value)
        AutoRaidType = value
        if value == "[Default : Above] Auto Farm Type : Above" then
    
            UI:Notify("Impact Hub", "To Use This Type.\n You Must Sliding The Distance More Than 0")
        elseif value == "[Default : Above] Auto Farm Type : Bellow" then
    
            UI:Notify("Impact Hub", "To Use This Type.\n You Must Sliding The Distance Lower Than 0")
        end
    end)

RaidWeapon = {}
for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do  
