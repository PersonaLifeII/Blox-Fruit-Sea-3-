    SelectIsZone = value
end)

Teleport:addButton("Teleport To Island / Zone", function(a)
    if SelectIsZone == "Floresco" then
        TP(CFrame.new(-3675, 57, 169))
    elseif SelectIsZone == "Viridans" then
        TP(CFrame.new(-5128, 57, 1878))
    elseif SelectIsZone == "Torrefacio" then
        TP(CFrame.new(-5226, 57, 798))
    elseif SelectIsZone == "Hibernus" then
        TP(CFrame.new(-4547, 136, -889))
    elseif SelectIsZone == "Carcer" then
        TP(CFrame.new(-4171, 29, 744))
    elseif SelectIsZone == "Skull Island" then
        TP(CFrame.new(-6516, 58, 5883))
    elseif SelectIsZone == "Dead Tundra" then
        TP(CFrame.new(1178, 14, 904))
    elseif SelectIsZone == "Loaf Island" then
        TP(CFrame.new(-505, 31, 8112))
    elseif SelectIsZone == "Shred Endangering" then
        TP(CFrame.new(-161, 201, -2935))
    elseif SelectIsZone == "Pirate Skull Island" then
        TP(CFrame.new(-9276, 58, -5039))
    elseif SelectIsZone == "Soldier Headquater" then
        TP(CFrame.new(-9696, 38, 955))
    elseif SelectIsZone == "" or SelectIsZone == nil then
        UI:Notify("Impact Hub", "Please Select Island / Zone")
    end
end)

