     BountyWeaponName = a
 end)
 
 DisBounty = 9
 BountySet:addSlider("Distance", 9,-50,200, function(a)
    DisBounty = a
 end)

 BountySet:addToggle("Auto Equip Sword", nil, function(value)
    _G.AutoEquip_SW = value
 end)


 task.spawn(function()
    while task.wait() do
        if _G.AutoEquip_SW == true then
        pcall(function()
           for i,v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
              if v.ClassName == "Tool" then
                  if v.ToolTip == "Sword" then
             if game.Players.LocalPlayer.Backpack:FindFirstChild(tostring(v.Name)) then
                local ToolSe = tostring(v.Name)
                local tool = game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe)
                wait(.4)
                game.Players.LocalPlayer.Character.Humanoid:EquipTool(tool)
            end
        end
    end
end
end)
end
end
end)

 BountySet:addToggle("Auto Equip Melee", nil, function(value)
    _G.AutoEquip_FS = value
 end)

    task.spawn(function()
        while task.wait() do
            if _G.AutoEquip_FS == true then
            pcall(function()
           for i,v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
              if v.ClassName == "Tool" then
                  if v.ToolTip == "Combat" then
             if game.Players.LocalPlayer.Backpack:FindFirstChild(tostring(v.Name)) then
                 local ToolSe = tostring(v.Name)
                local tool = game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe)
                wait(.4)
                game.Players.LocalPlayer.Character.Humanoid:EquipTool(tool)
            end
        end
    end
end
end)
end
end
end)


 local BountySec = page7:addSection("Bounty")

 BountySec:addToggle("Auto Farm Bounty", nil ,function(value)
    _G.AutoFarmBounty = value
    if value == false then
        UnClip()
    end
end)


spawn(function()
    while wait() do
        pcall(function()
            if _G.AutoFarmBounty then
                local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
                for i,v in pairs(game:GetService("Players").Character:GetChildren()) do
                    if v.Name ~= game.Players.LocalPlayer.Name then
                        if v:WaitForChild("Humanoid").Health > 0 and (plr.Position - v.HumanoidRootPart.Position).Magnitude <= 17000 then

                            MyEnemiesHealth = v.Humanoid.Health
                            repeat task.wait()
                                EnemiesName = v.Name
                                plr.CFrame = v.HumanoidRootPart.CFrame * Bounty_Mode

                                if not _G.FarmBountyConfig then
                                    EquipWeapon1(MainWeaponName)
                                    Haki()
                                end

                                spawn(function()
                                    if (v.HumanoidRootPart.Position - plr.Position).Magnitude <= 150 then
                                        EnemiesPos = true
                                    end
                                end)

                            MyEnemiesHumanoid2 = v.Humanoid
                            until _G.AutoFarmBounty == false or v.Humanoid.Health == 0 or not v:FindFirstChild("HumanoidRootPart") or not v:FindFirstChild("Humanoid") or not v.Parent or NextplySelect == true
                            EnemiesDead = false
                            EnemiesPos = false
                        end
                    end
                end
            end
        end)
    end
end)

spawn(function()
    pcall(function()
        while wait() do
            if _G.AutoFarmBounty then
                if MyEnemiesHumanoid2 ~= nil then
                    if EnemiesPos then
                        wait(5)
                        if MyEnemiesHumanoid2.Health == MyEnemiesHumanoid2.MaxHealth or MyEnemiesHumanoid2.Health >= MyEnemiesHealth then
                            EnemiesDead = true
                            MyEnemiesHumanoid2 = nil
                        end
                    end
                end
            end
        end
    end)
end)

 BountySec:addToggle("Auto Hop", nil,function(value)
    _G.AutoHop2 = value
 end)

 HopSec = 80
 BountySec:addSlider("Hop Delay", 80,5,500, function(a)
    HopSec = a
 end)

 task.spawn(function()
    while task.wait() do
        if _G.AutoHop2 then
        pcall(function()
            wait(HopSec)
            Hop()
        end)
     end
    end
end)
 
