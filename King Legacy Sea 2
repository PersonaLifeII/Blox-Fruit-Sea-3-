 local PLY = UI:addPage("Player\nEsp", 7252023075)
 local PLYSETTING = PLY:addSection("Player Setting")
 
 DisPlayer = 8
 PLYSETTING:addSlider("Distance", 8,-50,200, function(a)
     DisPlayer = a
 end)

 local PLYS = PLY:addSection("Players")

local playerlist = {}

for i,v in pairs(game.Players:GetPlayers())do
    if v ~= game.Players.LocalPlayer then
        table.insert(playerlist,v.Name)
  end
end

local drop = PLYS:addDropdown("Select Players : None", playerlist, function(a)
   Select = a
end)

game.Players.PlayerAdded:Connect(function(player)
    local name = player.Name
    table.insert(playerlist,name)
    drop:Refresh(playerlist)
end)

game.Players.PlayerRemoving:Connect(function(player)
    local name = player.Name
    for i,v in pairs(playerlist)do
        if v == name then  
            table.remove(playerlist,i)
        end
    end
    drop:Refresh(playerlist)
end)

PLYS:addToggle("Spectate Player", nil, function(value)
    SeeYouStatus = value
    local plr1 = game.Players.LocalPlayer.Character.Humanoid
    local plr2 = game.Players:FindFirstChild(Select)
    repeat wait(.1)
       game.Workspace.Camera.CameraSubject = plr2.Character.Humanoid
    until SeeYouStatus == false 
    game.Workspace.Camera.CameraSubject = game.Players.LocalPlayer.Character.Humanoid
 end)
 
 PLYS:addToggle("Kill Player", nil, function(Value)
 _G.KillPl = Value
 if Value == false then
    UnClip()
 elseif Value == true then
 if Select == nil then
    UI:Notify("Impact Hub", "Please Select Players")
 end
    UI:Notify("Impact Hub", "This Function Is Can Be Using In Behind Type Only")
end
end)


    task.spawn(function()
            while task.wait() do
                if _G.KillPl == true then
                pcall(function()
                    local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
                    if game:GetService("Workspace").PlayerCharacters:FindFirstChild(Select) then

                for i, v in pairs(game:GetService("Workspace").PlayerCharacters:GetChildren()) do
            if v.Name == Select and v.Humanoid.Health > 0 then
                plr.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,DisPlayer)
                    Haki()
                  end
                end
            end
        end)
             end
            end
        end)


