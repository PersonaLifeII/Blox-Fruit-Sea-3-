local FARM6 = page1:addSection("Dragon")


FARM6:addToggle("Auto Farm Dragon", nil, function(value)
    _G.Dragon = value
if value == false then
        UnClip()
elseif value == true then
    if AutoFarmType == "[Default : Above] Auto Farm Type : Bellow" or "[Default : Above] Auto Farm Type : Above" then
         UI:Notify(HubName, "This Function Is Can Be Using In Behind Type Only")
      end
   end
end)

    task.spawn(function()
        while task.wait() do
            if _G.Dragon then
            pcall(function()
                local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
                if not game:GetService("Workspace").Monster.Boss:FindFirstChild("Dragon [Lv. 5000]") then
                    plr.CFrame = game:GetService("ReplicatedStorage").MOB:FindFirstChild("Dragon [Lv. 5000]").HumanoidRootPart.CFrame * CFrame.new(0,80,0)
                end

            if game:GetService("Workspace").Monster.Boss:FindFirstChild("Dragon [Lv. 5000]") then
                for i, v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
                        if v.Name == "Dragon [Lv. 5000]" and v:FindFirstChild("Humanoid") then
                            if v.Humanoid.Health > 0 then
                            plr.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,DisFarm)
                            EquipWeapon1(MainWeaponName)
                            Haki()
                            elseif v.Humanoid.Health <= 0 then
                                plr.CFrame = CFrame.new(-6025, 442, 7444)
                                    end
                                end
                            end
                        end
                    end)
            end
        end
    end)

