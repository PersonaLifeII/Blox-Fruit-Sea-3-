FARM9:addToggle("Auto Permission [QUEST]", nil, function(value)
    _G.PermissionQ = value
    if value == false then
        UnClip()
    end
    if value == true then
    CheckQuest()
    if MyLevel <= 3400 then
        UI:Notify("Impact Hub", "Level 3400 Require")
     end
    if MainWeaponName == nil then
        UI:Notify("Impact Hub", "Please Select Your Weapon") 
    end
end
end)

task.spawn(function()
    while task.wait() do
 if _G.PermissionQ then
    pcall(function()
    local plr = game.Players.LocalPlayer.Character.HumanoidRootPart

    EquipWeapon1(MainWeaponName)
    Haki()

    if game:GetService("Players").LocalPlayer.PlayerGui.Quest.QuestBoard.Visible == false then
        
        if game:GetService("Workspace").AllNPC:FindFirstChild("QuestLvl3400") then
            plr.CFrame = game:GetService("Workspace").AllNPC["QuestLvl3400"].HumanoidRootPart.CFrame * CFrame.new(0,-5,0)
            wait()
            
            UIClick()
        else 
            plr.CFrame = game:GetService("ReplicatedStorage").MAP["QuestLvl3400"].HumanoidRootPart.CFrame * CFrame.new(0,-5,0)
            wait()

            UIClick()
         end    
        

         CheckQuest()
          if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("QuestLvl3400") then
            if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("QuestLvl3400").Dialogue then
                if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("QuestLvl3400").Dialogue.Accept then

                    game:GetService("Players").LocalPlayer.PlayerGui["QuestLvl3400"].Dialogue.Accept.Size = UDim2.new(0, 10000, 0, 10000)
                    game:GetService("Players").LocalPlayer.PlayerGui["QuestLvl3400"].Dialogue.Accept.Position = UDim2.new(-2, 0, -5, 0)
                    game:GetService("Players").LocalPlayer.PlayerGui["QuestLvl3400"].Dialogue.Accept.ImageTransparency = 1
            end
        end
    end
end

if not game:GetService("Workspace").Monster.Mon:FindFirstChild("Dark Beard Servant [Lv. 3400]") then
    plr.CFrame = CFrame.new(-9059, 59, -4558)
end


if game:GetService("Players").LocalPlayer.PlayerGui.Quest.QuestBoard.Visible == true then
    CheckQuest()
if game:GetService("Workspace").Monster.Mon:FindFirstChild("Dark Beard Servant [Lv. 3400]") then
    for i, v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do
      if v.Name == "Dark Beard Servant [Lv. 3400]" then
        if v.Humanoid.Health > 0 then
        plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
    elseif v.Humanoid.Health <= 0 then
        v:Destroy() break;
        end
    end
end
    CheckQuest()
    elseif game:GetService("Workspace").Monster.Boss:FindFirstChild("Dark Beard Servant [Lv. 3400]") then
            for i, v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
              if v.Name == "Dark Beard Servant [Lv. 3400]" then
                if v.Humanoid.Health > 0 then
                plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
                elseif v.Humanoid.Health <= 0 then
                    v:Destroy() break;
                end
            end
        end  
    end
end
end)
end
end
end)

