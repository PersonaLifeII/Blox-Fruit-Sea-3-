function KenHaki()
    local pl = game.Players.LocalPlayer.Character
  if pl.KenOpen.Value == false then
    game:GetService("Players").LocalPlayer.Character.Services.Client.KenEvent:InvokeServer()
        print("Ken Haki")
         end
      end

 function Haki()
    local pl = game.Players.LocalPlayer.Character

    if not pl:FindFirstChild("ArmamentBodyGroup") then
        if pl.Haki.Value == 0 then
            pl.Haki.Value += 1
            game:GetService("Players").LocalPlayer.Character.Services.Client.Armament:FireServer()
            print("Haki")
        end
end
end



