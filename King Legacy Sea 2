local Venyx = loadstring(game:HttpGet("https://raw.githubusercontent.com/PersonaLifeII/Impact-Hub/main/UI%20Venyx"))()
local UI  = Venyx.new("Impact Hub | Premium | Revamp Script | "..os.date("%d".." ".."%B".." ".."%Y"), 5013109572) -- date

local page1 = UI:addPage("Main Tab", 7040391851)
local FARMS = page1:addSection("Auto Farm Setting")


spawn(function()
while wait(.1) do
    if AutoFarmType == "[Default : Above] Auto Farm Type : Above" then
        Farm_Mode = CFrame.new(0,DisFarm,-1) * CFrame.Angles(math.rad(-90),0,0)
    elseif AutoFarmType == "[Default : Above] Auto Farm Type : Bellow" then
        Farm_Mode = CFrame.new(0,DisFarm,1) * CFrame.Angles(math.rad(90),0,0)
    elseif AutoFarmType == "[Default : Above] Auto Farm Type : Behind" then
        Farm_Mode = CFrame.new(0,0,DisFarm) * CFrame.Angles(math.rad(0),0,0)
    end
end
end)

FarmTable = {
"[Default : Above] Auto Farm Type : Above",
"[Default : Above] Auto Farm Type : Bellow",
"[Default : Above] Auto Farm Type : Behind"
}

-- AutoFarmType = "[Default : Above] Auto Farm Type : Above"
AutoFarmType = FarmTable[1]

FARMS:addDropdown(""..AutoFarmType, FarmTable, function(value)
    AutoFarmType = value
    if value == "[Default : Above] Auto Farm Type : Above" then

        UI:Notify("Impact Hub", "To Use This Type.\n You Must Sliding The Distance More Than 0")
    elseif value == "[Default : Above] Auto Farm Type : Bellow" then

        UI:Notify("Impact Hub", "To Use This Type.\n You Must Sliding The Distance Lower Than 0")
    end
end)

MainWeaponList = {}
for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do  
    if v:IsA("Tool") then
        table.insert(MainWeaponList ,v.Name)
    end
end

FARMS:addDropdown("Select Weapon : None", MainWeaponList, function(value)
    MainWeaponName = value
end)

_G.BringMonster = true
FARMS:addToggle("[ First Hub! ] Bring Monster", true, function(value)
    _G.BringMonster = value

    if value == false then
        UnClip()
    end
end)


FARMS:addToggle("[Reduce Lag] White Screen", nil, function(value)
    WhiteScreen = value
    if WhiteScreen then
    game:GetService("RunService"):Set3dRenderingEnabled(false)

    else
        game:GetService("RunService"):Set3dRenderingEnabled(true)
        UnClip()
    end
    
end)


DisFarm = 13
FARMS:addSlider("Distance", 13,-50,200, function(a)
    DisFarm = a
end)

local FARM = page1:addSection("Auto Farm")

FARM:addToggle("Auto Farm Level", nil, function(value)
    _G.LvFarm = value
if value == false then
UnClip()
CheckQuest()
end
if MainWeaponName == nil then
UI:Notify("Impact Hub", "Please Select Your Weapon") 
end
end)

task.spawn(function()
    while task.wait() do
 if _G.LvFarm then
    pcall(function()
    local plr = game.Players.LocalPlayer.Character.HumanoidRootPart

    game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed += 20

    EquipWeapon1(MainWeaponName)
    Haki()

    if game:GetService("Players").LocalPlayer.PlayerGui.Quest.QuestBoard.Visible == false then
        if game:GetService("Workspace").AllNPC:FindFirstChild("QuestLvl"..levelquest) then
            plr.CFrame = game:GetService("Workspace").AllNPC["QuestLvl"..levelquest].HumanoidRootPart.CFrame * CFrame.new(0,-5,0)
        else 
            plr.CFrame = game:GetService("ReplicatedStorage").MAP["QuestLvl"..levelquest].HumanoidRootPart.CFrame * CFrame.new(0,-5,0)
         end    

         UIClick()

         CheckQuest()
          if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("QuestLvl"..levelquest) then
            if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("QuestLvl"..levelquest).Dialogue then
                if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("QuestLvl"..levelquest).Dialogue.Accept then

                    game:GetService("Players").LocalPlayer.PlayerGui["QuestLvl"..levelquest].Dialogue.Accept.Size = UDim2.new(0, 10000, 0, 10000)
                    game:GetService("Players").LocalPlayer.PlayerGui["QuestLvl"..levelquest].Dialogue.Accept.Position = UDim2.new(-2, 0, -5, 0)
                    game:GetService("Players").LocalPlayer.PlayerGui["QuestLvl"..levelquest].Dialogue.Accept.ImageTransparency = 1
            end
        end
    end
end
if game:GetService("Players").LocalPlayer.PlayerGui.Quest.QuestBoard.Visible == true then
    CheckQuest()
if game:GetService("Workspace").Monster.Mon:FindFirstChild(Ms) then
    for i, v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do
      if v.Name == Ms then
        if v.Humanoid.Health > 0 then
        plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
    elseif v.Humanoid.Health >= 0 then
        v:Destroy() break;
        end
    end
end
    CheckQuest()
    elseif game:GetService("Workspace").Monster.Boss:FindFirstChild(Ms) then
            for i, v in pairs(game:GetService("Workspace").Monster.Boss:GetChildren()) do
              if v.Name == Ms then
                if v.Humanoid.Health > 0 then
                plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
                elseif v.Humanoid.Health >= 0 then
                    v:Destroy() break;
                end
            end
        end  
    end
end
end)
end
end
end)


local mobs = {} 

for _,v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do
    insert = true 
    for _,v2 in pairs(mobs) do if v2 == v.Name then insert = false end end 
    if insert then table.insert(mobs, v.Name) end 
end

local mobdropdown = FARM:addDropdown("Select Monster : None", mobs, function(value)
    MonSelected = value
end)

FARM:addToggle("Auto Farm Monster", nil, function(value)
    _G.MonsFarm = value
    if value == false then
        UnClip()
    end
    if MainWeaponName == nil then
        UI:Notify("Impact Hub", "Please Select Your Weapon") 
    end
end)
 
task.spawn(function()
    while task.wait() do
        if _G.MonsFarm == true then
        pcall(function()
            local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
            game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed += 20

        if MonSelected == nil then
            wait(2)
            UI:Notify("Impact Hub", "Please Select The Monster")
        end

        if game:GetService("Workspace").Monster.Mon:FindFirstChild(MonSelected) then
            for i, v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do
            if v.Name == MonSelected and v:FindFirstChild("Humanoid") then
            if v.Humanoid.Health > 0 then
            plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
            EquipWeapon1(MainWeaponName)
            Haki()
            else
                v:Destroy() break; 
                        end
                    end
                end
            end
        end)
    end
end
end)
    
            
            game:GetService("Workspace").Monster.Mon.ChildAdded:Connect(function() 
                for _,v2 in pairs(mobs) do table.remove(mobs, _) end
                
                for _,v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do
                    insert = true 
                    for _,v2 in pairs(mobs) do if v2 == v.Name then insert = false end end
                    if insert then table.insert(mobs, v.Name) end 
                end
                mobdropdown:Refresh(mobs)
            end)
            
            game:GetService("Workspace").Monster.Mon.ChildRemoved:Connect(function() 
                for _,v2 in pairs(mobs) do table.remove(mobs, _) end 
                
                for _,v in pairs(game:GetService("Workspace").Monster.Mon:GetChildren()) do 
                    insert = true
                    for _,v2 in pairs(mobs) do if v2 == v.Name then insert = false end end 
                    if insert then table.insert(mobs, v.Name) end 
                end
                mobdropdown:Refresh(mobs)
            end)
   

    FARM:addButton("Check Server Time", function()
        local GameTime = math.floor(workspace.DistributedGameTime+0.5)
        local Hour = math.floor(GameTime/(60^2))%24
        local Minute = math.floor(GameTime/(60^1))%60
        local Second = math.floor(GameTime/(60^0))%60

         UI:Notify("Impact Hub", "Total Time You Have Joined\nHour : "..Hour.." Minute : "..Minute.." Second : "..Second);
    end)

    FARM:addButton("Teleport To Current Quest", function()
        CheckQuest()
        if not game:GetService("Workspace").AllNPC:FindFirstChild("QuestLvl"..levelquest) then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("ReplicatedStorage").MAP["QuestLvl"..levelquest].HumanoidRootPart.CFrame
         else 
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").AllNPC["QuestLvl"..levelquest].HumanoidRootPart.CFrame
         end
    end)

    _G.Buso = true
    FARM:addToggle("Auto Armament Haki",  true, function(a)
        _G.Buso = a
            end)
    FARM:addToggle("Auto Observation Haki",  nil, function(a)
        _G.KenHaki = a
    end)

local FARM_NOTIFY = page1:addSection("Boss Noification")

_G.BossNotify = true
FARM_NOTIFY:addToggle("Boss Notify", true, function(value)
    _G.BossNotify = value
    if value == false then
    _G.SoundCheck = false
    end
end)

task.spawn(function()
    while task.wait() do
        if _G.BossNotify then
        pcall(function()
                 FindGayIsland()
               FindHDIsland()
              FindGS()
            FindRaidBoss()
            game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed += 20
        end)
     end
    end
end)

FARM_NOTIFY:addToggle("Boss Notify Sound", nil, function(value)
    _G.BossNotify_SoundAdd = value
    if value == false then
        _G.SoundCheck = false
    end
end)

task.spawn(function()
while task.wait() do
if _G.BossNotify_SoundAdd then
pcall(function()

    game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed += 20

if _G.SoundCheck == true then
wait(3)
so = Instance.new("Sound", game:GetService("Workspace"))
so.Name = "Boss_Notify"
so.Playing = _G.BossNotify_SoundAdd
so.Volume = 4
so.SoundId = "rbxassetid://1523818134"
so.Looped = false
end
end)
end
end
end)


local FARM2 = page1:addSection("Sea King")

FARM2:addToggle("Auto Farm Sea King", nil, function(value)
    _G.SeaKing = value
    if value == true then
    if AutoFarmType == "[Default : Above] Auto Farm Type : Bellow" or "[Default : Above] Auto Farm Type : Above" then
        UI:Notify("Impact Hub", "This Function Is Can Be Using In Behind Type Only")
    end
    if game:GetService("Workspace").SeaMonster:FindFirstChild("SeaKing") then
    UI:Notify("Impact Hub", "Recommended For Distance 200")
    end
    elseif value == false then
        UnClip()
      end
      if MainWeaponName == nil then
        UI:Notify("Impact Hub", "Please Select Your Weapon") 
    end
end)

task.spawn(function()
    while task.wait() do
        if _G.SeaKing then
        pcall(function()
            local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
            game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed += 20
            if not game:GetService("Workspace").SeaMonster:FindFirstChild("SeaKing") then
            TPGAYIsland()
        end
        if game:GetService("Workspace").SeaMonster:FindFirstChild("SeaKing") then
            for i,v in pairs(game:GetService("Workspace").SeaMonster:GetChildren()) do
                   if v.Name == "SeaKing" and v:FindFirstChild("Humanoid") then
                    if v.Humanoid.Health > 0 then
                    plr.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,DisFarm)
                    EquipWeapon1(MainWeaponName)
                    Haki()
                    end
                end
            end
        end
        if _G.Farm_Lv then
            if game:GetService("Workspace").SeaMonster:FindFirstChild("SeaKing").Humanoid.Helath <= 0 then
            wait(3)
            _G.Farm_Lv = true
            else
                _G.Farm_Lv = false
            end
        end
    end)
     end
    end
end)

FARM2:addToggle("Auto Grab Chest", nil, function(a)
    _G.GrabChest = a
end)

    task.spawn(function()
        while task.wait() do
            if _G.GrabChest then
            pcall(function()
                local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
                game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed += 20

                if game:GetService("Workspace").SeaMonster:FindFirstChild("SeaKing") then
                    if game:GetService("Workspace").SeaMonster:FindFirstChild("SeaKing").Humanoid.Health <= 0 then

                    if game:GetService("Workspace").Island:FindFirstChild("Legacy Island1") then
                        plr.CFrame = game:GetService("Workspace").Island["Legacy Island1"].ChestSpawner.CFrame

                    elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island2") then
                        plr.CFrame = game:GetService("Workspace").Island["Legacy Island2"].ChestSpawner.CFrame

                    elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island3") then
                        plr.CFrame = game:GetService("Workspace").Island["Legacy Island3"].ChestSpawner.CFrame

                    elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island4") then
                        plr.CFrame = game:GetService("Workspace").Island["Legacy Island4"].ChestSpawner.CFrame
                      end
                    end
                end
            end)
     end
    end
end)

FARM2:addToggle("Auto Find Sea King Server", nil, function(value)
    _G.SkHop = value
    if value == true then
        UI:Notify("Impact Hub", "This Function Will Active When Your Server Does't Have Legacy Island")
    end
end)

task.spawn(function()
    while task.wait() do
        if _G.SkHop then
        pcall(function()
            if not game:GetService("Workspace").Island:FindFirstChild("Legacy Island1") then
                UI:Notify("Impact Hub", "If The Game Have Tell You Server Is Full. Don't Care About It. Just Wait....")
                print("Not 1")
                wait(1)
                Hop()
                end
                if not game:GetService("Workspace").Island:FindFirstChild("Legacy Island2") then
                UI:Notify("Impact Hub", "If The Game Have Tell You Server Is Full. Don't Care About It. Just Wait....")
                print("Not 2")
                wait(1)
                Hop() 
                end
                if not game:GetService("Workspace").Island:FindFirstChild("Legacy Island3") then
                UI:Notify("Impact Hub", "If The Game Have Tell You Server Is Full. Don't Care About It. Just Wait....")
                print("Not 3")
                wait(1)
                Hop()
                end
            if not game:GetService("Workspace").Island:FindFirstChild("Legacy Island4") then
                UI:Notify("Impact Hub", "If The Game Have Tell You Server Is Full. Don't Care About It. Just Wait....")
                print("Not 4")
                wait(1)
                Hop()
            end
        end)
    end
end
end)

