end)

spawn(function()
    while true do game:GetService("RunService").RenderStepped:Wait()
        if _G.Ghost or _G.Trident or _G.Hell or _G.Mother or _G.Dragon or _G.Orbs or _G.Axe or _G.Knife or _G.SeaKing or _G.Float or _G.Cookie or _G.Sunken or _G.Pondere or _G.SoulCane or _G.KillPl or _G.Farm_OniMask or _G.TenguMask or _G.LvFarm or _G.Hydra or _G.Permission or _G.Permission2 or _G.Biscuit_Shoulder or _G.Blue_Scarf or _G.Lucidus or _G.Hydra or _G.KillRaidMon or _G.MonsFarm or _G.BlackBeard or _G.RandomDF then
                pcall(function()
                    if not game:GetService("Workspace"):FindFirstChild("Foot") then
                        local Foot = Instance.new("Part")
                        Foot.Name = "Foot"
                        Foot.Anchored = true
                        Foot.Material = "Neon"
                        Foot.Parent = game.Workspace
                        Foot.Size = Vector3.new(10,0.5,10)
                    end
                    if game:GetService("Workspace"):FindFirstChild("Foot") then
                        local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
                        game.Workspace["Foot"].CFrame = CFrame.new(plr.CFrame.X,plr.CFrame.Y - 4,plr.CFrame.Z)
                    end
                end)
            end
        end
    end)


    spawn(function()
        pcall(function()
            game:GetService("RunService").Stepped:Connect(function()
                if _G.Noclip or _G.Ghost or _G.Trident or _G.Hell or _G.Mother or _G.Dragon or _G.Orbs or _G.Axe or _G.Knife or _G.SeaKing or _G.Float or _G.Cookie or _G.Sunken or _G.Pondere or _G.SoulCane or _G.KillPl or _G.Farm_OniMask or _G.TenguMask or _G.LvFarm or _G.Hydra or _G.Permission or _G.Permission2 or _G.Biscuit_Shoulder or _G.Blue_Scarf or _G.Lucidus or _G.Hydra or _G.GrabHydraChest2 or _G.MonsFarm or _G.BlackBeard or _G.RandomDF then
                    for _, v in pairs(game:GetService("Players").LocalPlayer.Character:GetDescendants()) do
                        if v:IsA("BasePart") then
                            v.CanCollide = false    
                        end
                    end
                end
            end)
        end)
    end)




function UIClick()
    game:GetService'VirtualUser':Button1Down(Vector2.new(0,0.8))
    game:GetService'VirtualUser':Button1Up(Vector2.new(0,0.8))
end

function TPGAYIsland()
    local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
    if game:GetService("Workspace").Island:FindFirstChild("Legacy Island1") then
       plr.CFrame = game:GetService("Workspace").Island:FindFirstChild("Legacy Island1").ChestSpawner.CFrame * CFrame.new(0,100,0)
    elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island2") then
       plr.CFrame = game:GetService("Workspace").Island:FindFirstChild("Legacy Island2").ChestSpawner.CFrame * CFrame.new(0,100,0)
    elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island3") then
       plr.CFrame = game:GetService("Workspace").Island:FindFirstChild("Legacy Island3").ChestSpawner.CFrame * CFrame.new(0,100,0)
    elseif game:GetService("Workspace").Island:FindFirstChild("Legacy Island4") then
       plr.CFrame = game:GetService("Workspace").Island:FindFirstChild("Legacy Island4").ChestSpawner.CFrame * CFrame.new(0,100,0)
   end
end

function FindGayIsland()
    for i, v in pairs(game.Workspace.Island:GetChildren()) do
    if string.find(v.Name, "Legacy Island") and v:IsA("Model") and v:FindFirstChild("Main") then
        wait(2)
        UI:Notify(HubName, "Legacy Island Is Up From The Sea!")
        print("Legayc Island")
        _G.SoundCheck = true
        end
    end
end

function FindHDIsland()
    for i, v in pairs(game.Workspace.Island:GetChildren()) do
    if string.find(v.Name, "Sea King") and v:IsA("Model") and v:FindFirstChild("Main") then
        wait(2) 
        UI:Notify(HubName, "Hydra Island Is Up From The Sea!")
        print("Hydra Island")
        _G.SoundCheck = true
        end
    end
end

function TPHDIsland()
    local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
    if game:GetService("Workspace").Island:FindFirstChild("Sea King Water") then
        plr.CFrame = game:GetService("Workspace").Island["Sea King Water"].Main.CFrame * CFrame.new(0,120,0)
    elseif game:GetService("Workspace").Island:FindFirstChild("Sea King Thunder") then
       plr.CFrame = game:GetService("Workspace").Island["Sea King Thunder"].Main.CFrame * CFrame.new(0,120,0)
    elseif game:GetService("Workspace").Island:FindFirstChild("Sea King Lava") then
        plr.CFrame = game:GetService("Workspace").Island["Sea King Lava"].Main.CFrame * CFrame.new(0,120,0)
    end
end

function FindGS()
        if game:GetService("Workspace").GhostMonster:FindFirstChild("Ghost Ship") then
            wait(2)
            UI:Notify(HubName, "Ghost Ship Spawned!")
            print("Ghost Ship")
            _G.SoundCheck = true
        end
    end

    function FindRaidBoss()
        if game:GetService("ReplicatedStorage").MOB:FindFirstChild("Dragon [Lv. 5000]") then
            _G.SoundCheck = true
            wait(2)
            UI:Notify(HubName, "Dragon Spawned!")
         elseif game:GetService("Workspace").Monster.Boss:FindFirstChild("Dragon [Lv. 5000]") then
            _G.SoundCheck = true
            wait(2)
            UI:Notify(HubName, "Dragon Spawned!")
            else
                _G.SoundCheck = false
            
-------------------------------------------------------------------- Ms. Mother ---------------------

        if game:GetService("ReplicatedStorage").MOB:FindFirstChild("Ms. Mother [Lv. 7500]") then
            _G.SoundCheck = true
            wait(2)
            UI:Notify(HubName, "Mother Spawned!")
        elseif game:GetService("Workspace").Monster.Boss:FindFirstChild("Ms. Mother [Lv. 7500]") then
            _G.SoundCheck = true
            wait(2)
            UI:Notify(HubName, "Mother Spawned!")
        else
            _G.SoundCheck = false

------------------------------------------------------------------- King Samurai ---------------------

            if game:GetService("ReplicatedStorage").MOB:FindFirstChild("King Samurai [Lv. 3500]") then
                _G.SoundCheck = true
                wait(2)
               UI:Notify(HubName, "King Samurai Spawned!")
             elseif game:GetService("Workspace").Monster.Boss:FindFirstChild("King Samurai [Lv. 3500]") then
                wait(2)
                 UI:Notify(HubName, "King Samurai Spawned!")
                 _G.SoundCheck = true
                else
                _G.SoundCheck = false
             end
            end
        end
    end



    task.spawn(function()
        while task.wait() do
            pcall(function()
                wait(.9)
                table.clear(MainWeaponList)
                for i,v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
                    if v:IsA("Tool") then
                   table.insert(MainWeaponList,v.Name)
                                   end
                                end
                        end)
                end
            end)

            task.spawn(function()
                while task.wait() do
                    pcall(function()
                        wait(.9)
                        table.clear(BountyWeaponList)
                        for i,v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
                            if v:IsA("Tool") then
                           table.insert(BountyWeaponList,v.Name)
                                       end
                                    end
                                end)
                        end
                    end)

                    task.spawn(function()
                        while task.wait() do
                            pcall(function()
                                wait(.9)
                                table.clear(RaidWeapon)
                                for i,v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
                                    if v:IsA("Tool") then
                                   table.insert(RaidWeapon,v.Name)
                                               end
                                            end
                                        end)
                                end
                            end)


