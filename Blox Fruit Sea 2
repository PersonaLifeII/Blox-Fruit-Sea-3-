-- Blox Fruit Sea 2
game.StarterGui:SetCore("SendNotification", {
    Title = "Impact Hub"; 
    Text = "Thank For Using..."; 
    Icon = ""; 
    Duration = 2;
})
wait(1)
game.StarterGui:SetCore("SendNotification", {
    Title = "Impact Hub"; 
    Text = "Blox Fruit"; 
    Icon = ""; 
    Duration = 4;
})
wait(1)
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/OptusN1/VENYX-UI-2/main/VENYX%20UI%20%3D-%3D"))()
local painx = library.new("Impact Hub | Blox Fruit [SEA 2]", 5013109572)


local mainpage = painx:addPage("Main Page", 7040391851)
local farmsection = mainpage:addSection("Auto Farm")
local statspage = painx:addPage("Auto Stats", 7040410130)
local statssection = statspage:addSection("Auto Stats")
statssection:addToggle("Melee", nil, function(esea)
    _G.StatsUp_Melee = esea
end)
statssection:addToggle("Defense", nil, function(esea)
    _G.StatsUp_Defense = esea
end)
statssection:addToggle("Sword", nil, function(esea)
    _G.StatsUp_Sword = esea
end)
statssection:addToggle("Gun", nil, function(esea)
    _G.StatsUp_Gun = esea
end)
statssection:addToggle("Blox Fruit", nil, function(esea)
    _G.StatsUp_BloxFruit = esea
end)
statssection:addSlider("Point Up", _G.PointUp, 3, 100, function(esea)
    _G.PointUp = esea
end)

spawn(function()
    game:GetService("RunService").RenderStepped:Connect(function()
        if  _G.StatsUp_Melee then
            local args = {
                [1] = "AddPoint",
                [2] = "Melee",
                [3] = _G.PointUp
            }
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        end
    if  _G.StatsUp_Defense then
            local args = {
                [1] = "AddPoint",
                [2] = "Defense",
                [3] = _G.PointUp
            }
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        end
    if  _G.StatsUp_Sword then
            local args = {
                [1] = "AddPoint",
                [2] = "Sword",
                [3] = _G.PointUp
            }
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        end
    if  _G.StatsUp_Gun then
            local args = {
                [1] = "AddPoint",
                [2] = "Gun",
                [3] = _G.PointUp
            }
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        end
    if  _G.StatsUp_BloxFruit then
            local args = {
                [1] = "AddPoint",
                [2] = "Demon Fruit",
                [3] = _G.PointUp
            }
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        end
    end)
end)
