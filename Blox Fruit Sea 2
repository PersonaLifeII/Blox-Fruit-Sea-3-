local tptoolsection = itpage:addSection("Teleport Tool")
tptoolsection:addButton("Teleport Tool", function()
mouse = game.Players.LocalPlayer:GetMouse()
tool = Instance.new("Tool")
tool.RequiresHandle = false
tool.Name = "Teleport Tool"
tool.Activated:connect(function()
local pos = mouse.Hit+Vector3.new(0,2.5,0)
pos = CFrame.new(pos.X,pos.Y,pos.Z)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
end)
tool.Parent = game.Players.LocalPlayer.Backpack
end)
local settingpage = painx:addPage("Setting", 5012544693)
local setsection = settingpage:addSection("Theme")
local themes = {
Background = Color3.fromRGB(24, 24, 24),
Glow = Color3.fromRGB(0, 0, 0),
Accent = Color3.fromRGB(10, 10, 10),
LightContrast = Color3.fromRGB(20, 20, 20),
DarkContrast = Color3.fromRGB(14, 14, 14),  
TextColor = Color3.fromRGB(255, 255, 255)
}


for theme, color in pairs(themes) do
setsection:addColorPicker(theme, color, function(color3)
painx:setTheme(theme, color3)
end)
end

painx:SelectPage(painx.pages[1], true)

    local keysection = settingpage:addSection("KeyBinds")
    keysection:addKeybind("Toggle Ui", Enum.KeyCode.RightControl, function()
    painx:toggle()
    end)

    function click()
        game:GetService'VirtualUser':CaptureController()
        game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672))
    end

    function BusoHaki()
        if game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
        else
        local args = {
            [1] = "Buso"
        }
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
    end
end

    function TP(P1)
        Distance = (P1.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
        if Distance < 250 then
            Speed = 750
        elseif Distance < 500 then
            Speed = 500
        elseif Distance < 1000 then
            Speed = 450
        elseif Distance >= 1000 then
            Speed = 300
        end
        game:GetService("TweenService"):Create(
            game.Players.LocalPlayer.Character.HumanoidRootPart,
            TweenInfo.new(Distance/Speed, Enum.EasingStyle.Linear),
            {CFrame = P1}
        ):Play()
    end
    
    function TP2(P1)
        Distance = (P1.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
        if Distance < 1000 then
            Speed = 400
        elseif Distance >= 1000 then
            Speed = 250
        end
        game:GetService("TweenService"):Create(
            game.Players.LocalPlayer.Character.HumanoidRootPart,
            TweenInfo.new(Distance/Speed, Enum.EasingStyle.Linear),
            {CFrame = P1}
        ):Play()
        Clip = true
        wait(Distance/Speed)
        Clip = false
    end

            local Camera = require(game.ReplicatedStorage.Util.CameraShaker)
            Camera:Stop()


            spawn(function()
                return (function(z1, z2, z3, z4, z5, z6, b1, b2)
                    local v = require
                    local k = task.wait
                    local d = pairs
                local up = getupvalues(require(game:GetService("Players").LocalPlayer.PlayerScripts.CombatFramework))
                local j1 = require(game.ReplicatedStorage.Util.CameraShaker)
                    j1:Stop()
                    local j = up[2]
                    spawn(
                        function()
                            while k() do
                                b1(
                                    function()
                                        if _G.FastAttack then
                                            j.activeController.attacking = z1
                                            j.activeController.timeToNextAttack = z2
                                            j.activeController.blocking = z3
                                            j.activeController.timeToNextBlock = z4
                                            j.activeController.humanoid.AutoRotate = z5
                                            j.activeController.increment = #j.activeController.anims.basic
                                            j.activeController.hitboxMagnitude = z6
                                        end
                                    end
                                )
                            end
                        end
                    )
                end)(false, -1, false, -1, false, 60, pcall, type)
                end)
