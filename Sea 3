local S1_4 = page1:NewSecction("Auto Farm Boss")

local BossSelected = S1_4:Dropdown("Select Boss", Bosslist, function(a)
_G.SelectBoss = a
end)
S1_4:Button("Refesh Boss", function()
    BossSelected:Clear()
    local Boss = {}
    for i, v in pairs(game.ReplicatedStorage:GetChildren()) do
        if string.find(v.Name, "Boss") then
            if v.Name == "Ice Admiral [Lv. 700] [Boss]" then
            else
                table.insert(Boss, v.Name)
                BossSelected:Add(v.Name)
            end
        end
    end
end)

