                        }),
                        utility:Create("TextBox", {
                            Name = "Textbox",
                            BackgroundTransparency = 1,
                            Position = UDim2.new(0.300000012, 0, 0, 0),
                            Size = UDim2.new(0.600000024, 0, 1, 0),
                            ZIndex = 2,
